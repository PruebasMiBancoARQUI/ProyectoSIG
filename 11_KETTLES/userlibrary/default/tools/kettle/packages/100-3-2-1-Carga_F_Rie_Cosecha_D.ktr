  <step>
    <name>Leo HD_DESEMBOLSO</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>SELECT TO_DATE(&apos;${FECHAPROCESOEDY_S_P}&apos;,&apos;YYYYMMDD&apos;) AS FE_PROCESO,
       TO_NUMBER(TO_CHAR(a.FE_DESEMBOLSO,&apos;YYYYMM&apos;)) AS NU_PERI_DESE,
       a.NU_PRESTAMO,
       TRUNC(MONTHS_BETWEEN(TO_DATE(&apos;${FECHAPROCESOEDY_S_P_M}01&apos;,&apos;YYYYMMDD&apos;), TO_DATE(TO_CHAR(a.FE_DESEMBOLSO,&apos;YYYYMM&apos;),&apos;YYYYMM&apos;))) AS NU_COSECHA,
       NVL(d.NU_PRES_FINA,a.NU_PRESTAMO) AS NU_PRES_FINA,
       DECODE(d.NU_PRES_FINA,NULL,c.CO_ESTA_OPER,e.CO_ESTA_OPER) AS CO_ESTA_PRES,
       TRIM(DECODE(d.NU_PRES_FINA,NULL,a.CO_REFINANCIACION,f.CO_REFINANCIACION)) AS CO_REFINANCIACION,
       a.MO_DESEMBOLSADO,
       CASE
          WHEN d.NU_PRES_FINA IS NULL     AND (c.NU_DIAS_ATRA &gt;= 30 OR c.CO_ESTA_OPER = &apos;C&apos;) THEN c.MO_SALD_CAPI_MN * NVL(d.VL_PORC_PART,1)
          WHEN d.NU_PRES_FINA IS NOT NULL AND (e.NU_DIAS_ATRA &gt;= 30 OR f.CO_REFINANCIACION = &apos;R&apos; OR e.CO_ESTA_OPER = &apos;C&apos;) THEN e.MO_SALD_CAPI_MN * NVL(d.VL_PORC_PART,1)
          ELSE 0
       END AS MO_DETERIORADO,
       CASE
          WHEN d.NU_PRES_FINA IS NULL     AND (c.NU_DIAS_ATRA &gt;= 30 OR c.CO_ESTA_OPER = &apos;C&apos;) THEN c.MO_SALD_CAPI_MN * NVL(d.VL_PORC_PART,1)
          WHEN d.NU_PRES_FINA IS NOT NULL AND (e.NU_DIAS_ATRA &gt;= 30 OR f.CO_REFINANCIACION = &apos;R&apos; OR e.CO_ESTA_OPER = &apos;C&apos;) THEN e.MO_SALD_CAPI_MN * NVL(d.VL_PORC_PART,1)
          ELSE 0
       END AS MO_DETE_FENO_NINO,
       CASE
          WHEN d.NU_PRES_FINA IS NULL     AND (c.NU_DIAS_ATRA &gt;= 15 OR c.CO_ESTA_OPER = &apos;C&apos;) THEN c.MO_SALD_CAPI_MN * NVL(d.VL_PORC_PART,1)
          WHEN d.NU_PRES_FINA IS NOT NULL AND (e.NU_DIAS_ATRA &gt;= 15 OR f.CO_REFINANCIACION = &apos;R&apos; OR e.CO_ESTA_OPER = &apos;C&apos;) THEN e.MO_SALD_CAPI_MN * NVL(d.VL_PORC_PART,1)
          ELSE 0
       END AS MO_DETE_15, 
       CASE
          WHEN d.NU_PRES_FINA IS NULL     AND (c.NU_DIAS_ATRA &gt;= 30 OR c.CO_ESTA_OPER = &apos;C&apos;) THEN c.MO_SALD_CAPI_MN * NVL(d.VL_PORC_PART,1)
          WHEN d.NU_PRES_FINA IS NOT NULL AND (e.NU_DIAS_ATRA &gt;= 30 OR f.CO_REFINANCIACION = &apos;R&apos; OR e.CO_ESTA_OPER = &apos;C&apos;) THEN e.MO_SALD_CAPI_MN * NVL(d.VL_PORC_PART,1)
          ELSE 0
       END AS MO_DETE_30,
       DECODE(d.NU_PRES_FINA,NULL,c.NU_DIAS_ATRA,e.NU_DIAS_ATRA) AS NU_DIAS_MORA, 
       SYSDATE AS FE_ACTU_DIA
FROM  ODS.HD_DESEMBOLSO a
      LEFT JOIN ODS.UD_EGP c            ON c.NU_PRESTAMO  = a.NU_PRESTAMO AND c.FE_SALDO = TO_DATE(&apos;${FECHAPROCESOEDY_S_P}&apos;,&apos;YYYYMMDD&apos;)
      LEFT JOIN BDS.F_RIE_REF_EXTREMO d ON d.NU_PRES_ORIG = a.NU_PRESTAMO
      LEFT JOIN ODS.UD_EGP e            ON e.NU_PRESTAMO  = d.NU_PRES_FINA AND e.FE_SALDO = TO_DATE(&apos;${FECHAPROCESOEDY_S_P}&apos;,&apos;YYYYMMDD&apos;)
      LEFT JOIN ODS.HD_DESEMBOLSO f     ON f.NU_PRESTAMO  = d.NU_PRES_FINA
      LEFT JOIN ODS.MD_SOLICITUDES g    ON g.NU_SOLICITUD = a.NU_SOLICITUD
      INNER JOIN ODS.MD_PRODUCTO_01 h   ON h.CO_PRODUCTO  = g.CO_PRODUCTO AND UPPER(h.DE_PRODUCTO) NOT LIKE &apos;%PRE%PAG%&apos;
      LEFT JOIN ODS.MD_EXTORNOS_DESEMBOLSOS i ON i.NU_PRESTAMO = a.NU_PRESTAMO
WHERE a.FE_DESEMBOLSO &lt; (
                         SELECT CASE
                                   WHEN TO_NUMBER(TO_CHAR(FECHAPROXIMOPROCESO,&apos;YYYYMM&apos;)) &gt; TO_NUMBER(TO_CHAR(FECHAPROCESO,&apos;YYYYMM&apos;)) -- Cuando es el último día de proceso del mes
                                   THEN TRUNC(FECHAPROCESO) + 1
                                   ELSE TO_DATE(TO_CHAR(FECHAPROCESO,&apos;YYYYMM&apos;)||&apos;01&apos;,&apos;YYYYMMDD&apos;)
		                        END AS FECHAPROCESO
                         FROM STG.T_PARAMETROS
                         WHERE FECHAPROCESO = TO_DATE(&apos;${FECHAPROCESOEDY_S_P}&apos;,&apos;YYYYMMDD&apos;)
                        )
      AND a.CO_REFINANCIACION NOT IN (&apos;R&apos;,&apos;P&apos;)
      AND i.NU_PRESTAMO IS NULL
</sql>
