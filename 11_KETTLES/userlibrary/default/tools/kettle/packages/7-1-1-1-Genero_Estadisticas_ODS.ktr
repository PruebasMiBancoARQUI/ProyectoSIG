    <connection>TopazDS</connection>
    <sql>SELECT
	CHR(39) || tge.ESQUEMA || CHR(39) AS ESQUEMA,
	CHR(39) || tge.NOMBRE_TABLA || CHR(39) AS NOMBRE_TABLA,
	tge.TIENE_PARTICION AS TIENE_PARTICION,
	tge.TIPO_PARTICION AS TIPO_PARTICION,
	CASE WHEN tge.TIENE_PARTICION=&apos;S&apos; THEN
			CASE WHEN tge.TIPO_PARTICION = &apos;DIARIA&apos; THEN CHR(39) || tge.NOMBRE_PARTICION || to_char(to_date(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;),&apos;YYYYMMDD&apos;) || CHR(39)
				 ELSE CHR(39) || tge.NOMBRE_PARTICION || to_char(to_date(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;),&apos;YYYYMM&apos;) || CHR(39)
			END 
		 ELSE NULL
 	END AS NOMBRE_PARTICION
FROM 
	TOPAZETL.TABLAS_GENERAR_ESTADISTICAS tge
WHERE
	tge.ESQUEMA=&apos;ODS&apos;</sql>
