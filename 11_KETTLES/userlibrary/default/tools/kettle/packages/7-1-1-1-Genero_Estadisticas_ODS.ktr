  <step>
    <name>Leo TABLAS_GENERAR_ESTADISTICAS</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
