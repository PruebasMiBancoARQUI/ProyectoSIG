    <connection>TopazRepDS</connection>
    <sql>WITH SUBQ1 AS (
        SELECT
            pd.NROASIENTOMOV AS NROASIENTOMOV,
            pd.SUCURSALMVTO AS SUCURSALMVTO,
            pd.FECHAPROCESOMOV AS FECHAPROCESOMOV
        FROM   
            EDYFICAR.BS_PAYS_DETAIL pd
        GROUP BY pd.NROASIENTOMOV, pd.SUCURSALMVTO, pd.FECHAPROCESOMOV      
)
SELECT 
	   DISTINCT
	   a.FECHAPROCESO AS FECHAPROCESO,
       a.SUCURSAL AS SUCURSAL,
       a.ASIENTO AS ASIENTO,
       a.OPERACION AS OPERACION,
       a.HORAFIN AS HORAFIN,
       a.INIUSR AS INIUSR,
       a.HORAINICIO AS HORAINICIO,
       a.DESCRIPCION AS DESCRIPCION
FROM EDYFICAR.ASIENTOS a
          JOIN SUBQ1 
             ON a.FECHAPROCESO=SUBQ1.FECHAPROCESOMOV
                AND a.SUCURSAL=SUBQ1.SUCURSALMVTO
                AND a.ASIENTO=SUBQ1.NROASIENTOMOV
WHERE 
    a.TZ_LOCK = 0
    AND   a.ESTADO = 77</sql>
