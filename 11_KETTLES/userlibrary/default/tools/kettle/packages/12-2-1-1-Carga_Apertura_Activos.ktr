    <connection>OdsStgDS</connection>
    <sql>WITH 
TMP_AHORRO as
--(SELECT S.C1803 AS CO_CLIENTE, S.JTS_OID AS IN_SALD_JTS_OID
--FROM STG.T_TMP_SALDOS_04 S
--JOIN STG.T_TMP_SALDOS_DIARIOS_01 SD ON S.JTS_OID = SD.SALDO_JTS_OID
--LEFT JOIN STG.T_TMP_HISTORIA_VISTA_EGD HV  ON HV.SALDO_JTS_OID = S.JTS_OID AND HV.FECHA_PROCESADO = TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)
--LEFT JOIN  STG.T_CO_PRODUCTOS PROD ON PROD.C6250=S.PRODUCTO AND PROD.C6252 = S.C9314   
--WHERE  S.C9314 IN (2,3) AND(SD.SALDO_AL_CORTE &lt;&gt; 0  OR (SD.SALDO_AL_CORTE = 0 AND SD.INTERESESPAGO &lt;&gt; 0))
--AND S.COD_CAN !=&apos;1&apos; AND PROD.C6252=3)  
(SELECT EGD.CO_CLIENTE AS CO_CLIENTE  FROM ODS.UD_EGD EGD WHERE IN_ESTA_PASI !=1
             AND  EGD.CO_TIPO_PASI=3  GROUP BY EGD.CO_CLIENTE )


SELECT DISTINCT
  TMP1.NU_PERI_MES,
  TMP1.FE_PROCESO,       
  TMP1.NU_PRESTAMO,      
  TMP1.CO_CLIENTE, 
    TMP1.IN_SIN_HIPO_INSC,
  TMP1.NU_PLAZ_DIAS,
  TMP1.FE_VENCIMIENTO,   
  TMP1.FE_DESEMBOLSO, 
  TMP1.MO_DESEMBOLSO,
  TMP1.NU_LINE_CRED,
  TMP1.IN_REFINANCIADO,
  TMP2.TI_PRESTAMO, 
  TMP2.CO_MONEDA,  
  TMP2.CO_SUCURSAL,       
  TMP2.CO_ANALISTA,
  TMP2.CO_USUA_TOPA,
  TMP2.CO_CARG_ANAL,
  TMP2.CO_PUES_ANAL,  
  TMP2.CO_SUCU_ANAL,       
  TMP2.CO_PRODUCTO,  
  TMP2.CO_CRITERIO,       
  TMP2.NU_SOLICITUD,  
  TMP2.NU_GRUP_SOLI,      
  TMP2.CO_SUCU_PADR, 
  TMP1.CO_ANAL_EVAL AS CO_ANAL_EVAL,
  TMP2.TI_CLIENTE,     
  nvl(TMP2.NU_CANT_EMPR,0) AS NU_CANT_EMPR,        
  nvl(TMP2.MO_SALD_SIST_FINA,0) AS MO_SALD_SIST_FINA, 
  nvl(TMP2.NU_PORC_EXCL,0) AS NU_PORC_EXCL ,    
  TMP2.NU_PERI_GRAC,  
  TMP2.CO_TIPO_GRAC,      
  TMP2.TI_EXCEPCION,
  TMP3.VL_TCEA,     
  ROUND((SELECT 
        CASE 
          WHEN PROD.C6253=&apos;E&apos; THEN TMP3.VL_TASA_MINI
               WHEN PROD.C6253=&apos;M&apos; THEN (((POWER((1+((TMP3.VL_TASA_MINI&#47;12)&#47;100)),12))-1)*100)
               WHEN PROD.C6253=&apos;N&apos; THEN TMP3.VL_TASA_MINI
            END
          FROM STG.T_CO_PRODUCTOS PROD
          WHERE PROD.C6250=TMP2.CO_PRODUCTO
  ),7) AS VL_TASA_MINI, 
  ROUND((SELECT 
        CASE 
          WHEN PROD.C6253=&apos;E&apos; THEN TMP3.VL_TASA_MAXI
               WHEN PROD.C6253=&apos;M&apos; THEN (((POWER((1+((TMP3.VL_TASA_MAXI&#47;12)&#47;100)),12))-1)*100)
               WHEN PROD.C6253=&apos;N&apos; THEN TMP3.VL_TASA_MAXI
            END
          FROM STG.T_CO_PRODUCTOS PROD
          WHERE PROD.C6250=TMP2.CO_PRODUCTO
  ),7) AS VL_TASA_MAXI,   
  TMP3.NU_CUOT_PACT, 
  TMP3.CO_USUA_APRO_SOLI,
  TMP3.IN_FOGAPI, 
  TMP3.ST_FOGAPI, 
  TMP3.IN_SALD_JTS_OID,  
  TMP3.IN_GARA_REAL,     
  TMP3.VL_TASA_ICM,       
  TMP3.VL_TASA_INTE_MOR,  
  TMP3.VL_TIPO_CAMB_OFIC,
  TMP4.CO_ACTI_ECON_INTE,
  TMP4.CO_SECT_ECON_ALTE,  
  TMP4.CO_ACTIVIDAD,
  TMP4.CO_ACTI_CIIU,  
  TMP4.CO_SECT_ECON_INTE,  
  TMP4.IN_EMPLEADO,
  TMP4.NU_EMPR_REPO,  
  TMP4.MO_TOTA_SIST_FINA,  
  (SELECT ps.CODAGENCIA FROM STG.T_SL_PRESOLICITUDES ps WHERE ps.NROSOLICITUD = TMP2.NU_SOLICITUD) AS CO_SUCU_PRES,
    TMP1.CO_INVERSION,
    TMP1.CO_FORM_DESE,
    TMP1.NU_CUEN_PASI,
    TMP1.FE_PRIM_VENC,
    TMP1.MO_APROBADO,
    NVL(TMP1.MO_APROBADO*TMP3.VL_TIPO_CAMB_OFIC,0) MO_APRO_SOLE,  
  TMP3.IN_GARA_ACTI,
  TMP1.MO_SALD_DISP_LINE,  
  NVL(TMP1.MO_DESEMBOLSO*TMP3.VL_TIPO_CAMB_OFIC,0) AS MO_DESE_SOLE,
CASE WHEN TMP1.CO_CLIENTE=AH.CO_CLIENTE THEN 1 ELSE 0 END AS IN_AHORRO,
  --NULL AS IN_AHORRO,
  TMP1.VL_TASA_OPTI,
  TMP1.CO_OPERATIVA,
  TMP1.CO_PROD_ORIG,
  TMP1.CO_COMP_ASEG AS CO_COMP_SEGU,
  TMP1.IN_SEGU_DESG,
  TMP1.IN_SEGU_DESG_EXTE,
  TMP1.VL_DESGRAVAMEN,
  TMP1.VL_TASA,
  TMP1.CO_REFINANCIADO,
  SYSDATE AS FE_ACTU_DIA  
FROM 
  STG.TMP_EGP_1 TMP1
  JOIN STG.TMP_EGP_2 TMP2 ON TMP1.SALDOS_JTS_OID=TMP2.SALDOS_JTS_OID
  JOIN STG.TMP_EGP_3 TMP3 ON TMP1.SALDOS_JTS_OID=TMP3.SALDOS_JTS_OID
  JOIN STG.TMP_EGP_4 TMP4 ON TMP1.SALDOS_JTS_OID=TMP4.SALDOS_JTS_OID
  LEFT JOIN TMP_AHORRO AH ON AH.CO_CLIENTE=TMP1.CO_CLIENTE</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
