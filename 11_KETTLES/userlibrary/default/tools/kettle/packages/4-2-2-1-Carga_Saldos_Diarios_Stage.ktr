    <connection>TopazRepDS</connection>
    <sql>SELECT
	sd.SALDO_JTS_OID AS SALDO_JTS_OID,
	sd.FECHA AS FECHA,
	sd.INT_CONTABILIZADOS AS INT_CONTABILIZADOS,
	sd.SALDO_AL_CORTE AS SALDO_AL_CORTE,
	sd.INTERESESPAGO AS INTERESESPAGO,
	sd.INT_REALIZADOS AS INT_REALIZADOS
FROM 
	 EDYFICAR.TMP_SALDOS_04 s 
	 JOIN EDYFICAR.TMP_SALDOS_DIARIOS_01 sd
	 	ON s.JTS_OID=sd.SALDO_JTS_OID
	 	AND sd.FECHA= TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)
WHERE
	s.TZ_LOCK=0
	AND sd.TZ_LOCK=0 
	AND s.C9314 IN (2,3)
----------------------
UNION ALL
----------------------
SELECT
	sd.SALDO_JTS_OID AS SALDO_JTS_OID,
	sd.FECHA AS FECHA,
	sd.INT_CONTABILIZADOS AS INT_CONTABILIZADOS,
	sd.SALDO_AL_CORTE AS SALDO_AL_CORTE,
	sd.INTERESESPAGO AS INTERESESPAGO,
	sd.INT_REALIZADOS AS INT_REALIZADOS
FROM 
	 EDYFICAR.TMP_SALDOS_03 s 
	 JOIN EDYFICAR.TMP_SALDOS_DIARIOS_01 sd
	 	ON s.JTS_OID=sd.SALDO_JTS_OID
	 	AND sd.FECHA= TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)
WHERE
	s.TZ_LOCK=0
	AND sd.TZ_LOCK=0 
	AND s.C9314 IN (4)</sql>
