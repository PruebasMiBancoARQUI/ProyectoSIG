  <step>
    <name>Leo T_MS_MICROSEG_CONTRATADOS</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>WITH 
CLI_PASI AS 
   (
      SELECT DISTINCT EGD.CO_CLIENTE AS CO_CLIENTE  
        FROM ODS.UD_EGD EGD 
       WHERE IN_ESTA_PASI    &lt;&gt; 1
         AND EGD.CO_TIPO_PASI = 3 
   ),
T_SOLICITUD AS
(SELECT &#47;*+ INDEX(SL.C5000) *&#47;SL.FLG_CON, SL.C5000,SL.C5002
          FROM STG.T_SL_SOLICITUDCREDITO SL 
         )
SELECT ms.NROCREDITO                AS NU_PRESTAMO,
       ms.MICROSEGURO               AS NU_MICROSEGURO,
       EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100+EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) AS NU_PERI_MES,
       ms.TIPO_MICROSEGURO          AS CO_TIPO_SEGU,
       ms.FECHA_INICIO              AS FE_INIC_VIGE,
       ms.FECHA_VTO                 AS FE_TERM_VIGE,
       (CASE WHEN MS.TITULARSEGURO = SU.CO_CLIENTE 
             THEN 1 
             ELSE 0 END)            AS IN_AHORRO,
       ms.NROPOLIZA                 AS NU_POLIZA,
       ms.MONTO                     AS MO_MICROSEGURO,
       ms.NROPRODUCTO               AS CO_PROD_MICR,
       SYSDATE                      AS FE_ACTU_DIA,
       ms.USUARIOVENTA              AS CO_USUA_TOPA,
       ms.TITULARSEGURO             AS CO_CLIENTE,
       ms.PLAZO                     AS NU_PLAZ_DIAS,
       ms.MONEDA                    AS CO_MONEDA,
       ms.NROSOLICITUD              AS NU_SOLICITUD,
       SL.FLG_CON                   AS IN_CONGLOMERADO,
       ms.ESTADO                    AS ST_MICROSEGURO,
       ms.FECHA_FIN                 AS FE_FIN_MICR,
       ms.FECHA_DESAFILIACION       AS FE_DESA_MICR
  FROM STG.T_MS_MICROSEG_CONTRATADOS MS
 INNER JOIN STG.T_SALDOS S ON MS.NROCREDITO = S.CUENTA AND S.C9314=5 AND S.OPERACION=0 AND S.ORDINAL=0
  LEFT JOIN CLI_PASI SU    ON SU.CO_CLIENTE = S.C1803
  LEFT JOIN T_SOLICITUD SL ON SL.C5000 = S.C1704 AND SL.C5002=ms.NROCREDITO</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
