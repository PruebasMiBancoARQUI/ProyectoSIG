  <step>
    <name>Leo T_SL_REL_SOL_DESTINO</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>SELECT 
	   EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100+EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) AS NU_PERI_MES,
       sl.SOLICITUD AS NU_SOLICITUD,
       sc.C5187 AS CO_CLIENTE, 
        s.CUENTA AS NU_PRESTAMO,
       sl.DESTINO AS CO_DESTINO,
       sl.PORCENTAJE AS VL_DESTINO,
       SYSDATE AS FE_ACTU_DIA
FROM 
	   STG.T_SL_REL_SOL_DESTINO sl
							JOIN STG.T_SL_SOLICITUDCREDITO sc
								ON sc.C5000 = sl.SOLICITUD
							JOIN STG.T_SALDOS s
								ON s.C1704 = sl.SOLICITUD
WHERE
	  sl.SOLICITUD&lt;&gt;0</sql>
