  <step>
    <name>Leo T_CR_LINEACREDITO</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>SELECT
  CRLC.NROLINEA AS NU_LINE_CRED,
  EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100+EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) AS NU_PERI_MES,
  CRLC.AGENCIA AS CO_SUCURSAL,
  CRLC.CODUS AS CO_ANALISTA,
  CRLC.CLTE AS CO_CLIENTE,
  CRLC.ESTADO AS CO_ESTA_LINE,
  CRLC.PROD AS CO_PRODUCTO,
  CRLC.ANCRED AS CO_USUA_TOPA,
  CRLC.CUOTA AS MO_CUOTA,
  CRLC.MODDISP AS CO_MODA_DISP,
  CRLC.MOTIVO AS DE_MOTI_CANC,
  CRLC.FECCAMBEST AS FE_CAMB_ESTA,
  CRLC.FECVTO AS FE_TERM_LINE_CRED,
  CRLC.FECAPER AS FE_INIC_LINE_CRED,
  CRLC.FECREVAL AS FE_REEVALUACION,
  CASE WHEN CRLC.CANCEL LIKE &apos;%S%&apos; THEN &apos;1&apos;
       WHEN CRLC.CANCEL LIKE &apos;%N%&apos; THEN &apos;0&apos;
       ELSE &apos;0&apos;
  END AS IN_CANCELADO,         
  CRLC.MTODISP AS MO_DISPONIBLE,
  CRLC.MTOOT AS MO_OTORGADO,
  CRLC.MTOUTIL AS MO_UTILIZADO,
  (SELECT COUNT(*) FROM STG.T_SALDOS s WHERE s.C1661=CRLC.NROLINEA)AS NU_DISP_LINE_CRED,
  CRLC.PLAZO AS NU_PLAZ_LINE_CRED,
  CRLC.PLAZDISP AS NU_PLAZ_MAXI_DISP,
  CRLC.NROSOL AS NU_SOLICITUD,
  SYSDATE AS FE_ACTU_DIA		
FROM STG.T_CR_LINEACREDITO CRLC
</sql>
