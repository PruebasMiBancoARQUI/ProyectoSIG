    USER_ASIGNMENT,
    FIRST_USER_ASIGNATION_DATE,
    UPDATE_DATE,
    INCOME,
    INCOME_PAYMENT_DAY,
    PERSONS_IN_CHARGE,
    BIRTH_DATE,
    LANGUAGE_ID,
    LAST_CONTACT_DAY,
    LAST_CONTACT_GROUP_ID,
    LAST_CONTACT_USER_ID,
    SECOND_SURNAME,
    MIDDLE_NAME,
    COMPANY,
    OCUPATION,
    PROFESSION,
    WORK_TIMETABLE_START,
    WORK_TIMETABLE_END,
