    <connection>IcsRepDS</connection>
    <sql>WITH
T_HISTORY AS
(SELECT B.RESPONSE_ID, B.CUSTOMER_ID, B.ACTION_ID
FROM COBRA.HISTORY B 
WHERE B.USER_ID &lt;&gt; &apos;SYSTEM&apos;
         AND B.CUSTOMER_ID = &apos;MIBANCO&apos;
         AND B.ACTION_ID NOT IN (&apos;ENTER TO CSCS&apos;,
                                 &apos;CHANGE STATE&apos;,
                                 &apos;IDENTITY CHANGE&apos;,
                                 &apos;CHANGE STATE&apos;,
                                 &apos;CHANGE DATA&apos;,
                                 &apos;CHANGE STRATEGY&apos;,
                                 &apos;CHANGE USER&apos;,
                                 &apos;DEMOGRAPHIC CHANGE&apos;)
   AND LENGTH(TRIM(TRANSLATE(B.USER_ID, &apos; 0123456789&apos;, &apos; &apos;))) IS NULL
         AND TRUNC(B.RECORD_DATE) = TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))
         
SELECT
    ACTION_ID,
    CUSTOMER_ID,
    LANGUAGE_ID,
    BLOCK,
    DESCRIPTION,
    PRIORITY,
    REPORT_DESCRIPTION,
    USER_NAME
FROM COBRA.ACTION A
    WHERE CUSTOMER_ID = &apos;MIBANCO&apos;
     AND LANGUAGE_ID = &apos;ESPANOL&apos;
     AND EXISTS (SELECT B.ACTION_ID
            FROM T_HISTORY B
           WHERE A.ACTION_ID = B.ACTION_ID
             AND B.CUSTOMER_ID = A.CUSTOMER_ID)
</sql>
