    <connection>IcsRepDS</connection>
    <sql>WITH DUPLICADOS AS (SELECT &#47;*+ PARALLEL(8) *&#47; IDENTITY_CODE, FECHA
  FROM (SELECT COUNT(1), IDENTITY_CODE, TO_CHAR(RECORD_DATE,&apos;DD&#47;MM&#47;YYYY HH24:MI:SS.FF3&apos;) FECHA
          FROM COBRA.HISTORY A 
         WHERE A.USER_ID &lt;&gt; &apos;SYSTEM&apos;
           AND A.CUSTOMER_ID = &apos;MIBANCO&apos;
           AND A.ACTION_ID NOT IN (&apos;ENTER TO CSCS&apos;,
                                         &apos;CHANGE STATE&apos;,
                                         &apos;IDENTITY CHANGE&apos;,
                                         &apos;CHANGE STATE&apos;,
                                         &apos;CHANGE DATA&apos;,
                                         &apos;CHANGE STRATEGY&apos;,
                                         &apos;CHANGE USER&apos;,
                                         &apos;DEMOGRAPHIC CHANGE&apos;)
           AND TO_DATE(TO_CHAR(A.RECORD_DATE,&apos;DD&#47;MM&#47;YYYY&apos;),&apos;DD&#47;MM&#47;YYYY&apos;) = TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)
         GROUP BY IDENTITY_CODE, TO_CHAR(RECORD_DATE,&apos;DD&#47;MM&#47;YYYY HH24:MI:SS.FF3&apos;)
        HAVING COUNT(1) &gt; 1))
SELECT
       IDENTITY_CODE,
       HISTORY_DATE,
       USER_ID,
       CUSTOMER_ID,
       ACTION_ID,
       GROUP_ID,
       SEQUENCE,
       RESPONSE_ID,
       CONTACT_ID,
       OBSERVATIONS,
       NEXT_ACTIVITY_DATE,
       DURATION,
       TELEPHONE_NUMBER,
       STRATEGY,
       PROCESS_TYPE,
       ACC_CUSTOMER_ID,
       ACCOUNT_NUMBER,
       PRODUCT_ID,
       REASON_ID,
       RESPONSE_WEIGHT,
       CONTACT_TYPE,
       RECORD_DATE,
       LATITUDE,
       LONGITUDE,
       MOBILE_STATUS_BATTERY,
       TO_DATE(TO_CHAR(A.HISTORY_DATE, &apos;DD&#47;MM&#47;YYYY&apos;), &apos;DD&#47;MM&#47;YYYY&apos;) AS FE_REGISTRO,
--       TO_DATE(SUBSTR(A.HISTORY_DATE,1, 6) || &apos;20&apos; || SUBSTR(A.HISTORY_DATE,7, 2),&apos;DD&#47;MM&#47;YYYY&apos;) AS FE_REGISTRO,
     SYSDATE AS FE_ACTU_DIA
FROM COBRA.HISTORY A
        WHERE A.USER_ID &lt;&gt; &apos;SYSTEM&apos;
         AND A.CUSTOMER_ID = &apos;MIBANCO&apos;
         AND A.ACTION_ID NOT IN (&apos;ENTER TO CSCS&apos;,
                                 &apos;CHANGE STATE&apos;,
                                 &apos;IDENTITY CHANGE&apos;,
                                 &apos;CHANGE STATE&apos;,
                                 &apos;CHANGE DATA&apos;,
                                 &apos;CHANGE STRATEGY&apos;,
                                 &apos;CHANGE USER&apos;,
                                 &apos;DEMOGRAPHIC CHANGE&apos;)
-- AND LENGTH(TRIM(TRANSLATE(A.USER_ID, &apos; 0123456789&apos;, &apos; &apos;))) IS NULL
   AND TO_DATE(TO_CHAR(A.RECORD_DATE,&apos;DD&#47;MM&#47;YYYY&apos;),&apos;DD&#47;MM&#47;YYYY&apos;) = TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)
   AND IDENTITY_CODE||TO_CHAR(RECORD_DATE,&apos;DD&#47;MM&#47;YYYY HH24:MI:SS.FF3&apos;)  NOT IN (SELECT IDENTITY_CODE||FECHA FROM DUPLICADOS)</sql>
