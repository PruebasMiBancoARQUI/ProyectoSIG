                                 &apos;DEMOGRAPHIC CHANGE&apos;)
   AND LENGTH(TRIM(TRANSLATE(B.USER_ID, &apos; 0123456789&apos;, &apos; &apos;))) IS NULL
         AND TRUNC(B.RECORD_DATE) = TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))
         
SELECT 
    IDENTITY_CODE,
    CUSTOMER_ID,
    STRATEGY,
    STRA_ENTRANCE_DATE,
