  <step>
    <name>Leo JTS_OD de Prest. Modif.</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>SELECT
	sm.SALDO_JTS_OID AS SALDOS_JTS_OID
FROM
(
	SELECT
		s.SALDO_JTS_OID AS SALDO_JTS_OID,
		s.C1604 AS C1604,
		nvl(sh2.C1604_HIST,0) AS C1604_HIST,
		s.C1608 AS C1608,
		nvl(sh2.C1608_HIST,0) AS C1608_HIST,
		s.C6393 AS C6393,
		nvl(sh2.C6393_HIST,0) AS C6393_HIST,
		s.C1711 AS C1711,
		nvl(sh2.C1711_HIST,0) AS C1711_HIST,
		s.ENTREGA_ACUENTA_ICM AS ENTREGA_ACUENTA_ICM,
		nvl(sh2.ENTREGA_ACUENTA_ICM_HIST,0) AS ENTREGA_ACUENTA_ICM_HIST,
		s.C1615 AS C1615,
		nvl(sh2.C1615_HIST,0) AS C1615_HIST,
		s.ICMORA_PERCIBIDO_SALD AS ICMORA_PERCIBIDO_SALD,
		nvl(sh2.ICMORA_PERCIBIDO_SALD_HIST,0) AS ICMORA_PERCIBIDO_SALD_HIST
	FROM STG.T_TMP_MONTOS_SALDOS s
		 LEFT JOIN 
			 ( SELECT 
			   		sh.SALDO_JTS_OID AS SALDO_JTS_OID,
			   		sh.C1604_HIST AS C1604_HIST,
			   		sh.C1608_HIST AS C1608_HIST,
			   		sh.C6393_HIST AS C6393_HIST,
			   		sh.C1711_HIST AS C1711_HIST,
			   		sh.ENTREGA_ACUENTA_ICM_HIST AS ENTREGA_ACUENTA_ICM_HIST,
			   		sh.C1615_HIST AS C1615_HIST,
			   		sh.ICMORA_PERCIBIDO_SALD_HIST AS ICMORA_PERCIBIDO_SALD_HIST   
			   	FROM STG.T_TMP_MONTOS_SALDOS_HIST sh 
			   	WHERE sh.FECHA_SALDO= (SELECT MAX (sh2.FECHA_SALDO) FROM STG.T_TMP_MONTOS_SALDOS_HIST sh2)
			  ) sh2 
		ON sh2.SALDO_JTS_OID=s.SALDO_JTS_OID
)  sm
WHERE
	sm.C1604 &lt;&gt; sm.C1604_HIST
	OR sm.C1608 &lt;&gt; sm.C1608_HIST
	OR sm.C6393 &lt;&gt; sm.C6393_HIST
	OR sm.C1711 &lt;&gt; sm.C1711_HIST
	OR sm.ENTREGA_ACUENTA_ICM &lt;&gt; sm.ENTREGA_ACUENTA_ICM_HIST
	OR sm.C1615 &lt;&gt; sm.C1615_HIST
	OR sm.ICMORA_PERCIBIDO_SALD &lt;&gt; sm.ICMORA_PERCIBIDO_SALD_HIST</sql>
