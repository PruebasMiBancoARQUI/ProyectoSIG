  <step>
    <name>Leo SALDOS FE_PSJE_VENC</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>TopazRepDS</connection>
    <sql>SELECT
  s.JTS_OID,
    (CASE WHEN (SELECT 1 
            FROM EDYFICAR.CR_CATEGORIAS_CARTERA cc
            WHERE cc.RUBRO_ORIGEN = s.C1692
            AND cc.CODIGO_ATRASO_DESTINO = &apos;C&apos;
            AND ROWNUM = 1) = 1
     THEN 
      CASE WHEN (SELECT 1 
             FROM EDYFICAR.DEVENGAMIENTO_POR_CUOTA dp
             WHERE dp.TZ_LOCK = 0
             AND dp.SALDO_JTS_OID = s.JTS_OID
             AND dp.RUBRO_CONTABLE LIKE &apos;14_5%&apos;
             AND ROWNUM = 1) = 1
         THEN (
            SELECT
              --dpc.SALDO_JTS_OID AS SALDO_JTS_OID,
              min(dpc.FECHA) AS FECHA
            FROM EDYFICAR.DEVENGAMIENTO_POR_CUOTA dpc
            WHERE
              dpc.TZ_LOCK=0
              AND dpc.RUBRO_CONTABLE LIKE &apos;14_5%&apos;
              AND dpc.SALDO_JTS_OID=s.JTS_OID
              AND ROWNUM&lt;2
            GROUP BY dpc.SALDO_JTS_OID
             &#47;*SELECT FECHA 
             FROM STG.T_DEVENGAMIENTO_CUOTA_SUMMARY DPCSUMMARY
             WHERE DPCSUMMARY.SALDO_JTS_OID=s.JTS_OID*&#47;
        )              
         ELSE (s.C1628+(SELECT cc2.INCUMPLIMIENTOS FROM EDYFICAR.CR_CATEGORIAS_CARTERA cc2 WHERE cc2.CODIGO_ATRASO_DESTINO = &apos;N&apos; AND cc2.RUBRO_ORIGEN = s.C1692 AND ROWNUM&lt;2))
      END
     ELSE
      CASE WHEN (SELECT 1 
             FROM EDYFICAR.BS_HISTORIA_PLAZO hp
             WHERE hp.TZ_LOCK=0
             AND hp.SALDOS_JTS_OID= s.JTS_OID
             AND hp.TIPOMOV=&apos;R&apos;
             AND hp.RUBROCONTABLE LIKE &apos;14_5%&apos;
             AND ROWNUM = 1) = 1
         THEN (
            SELECT
              --hp.SALDOS_JTS_OID AS SALDOS_JTS_OID,
              min(hp.FECHAVALOR) AS FECHA
            FROM EDYFICAR.BS_HISTORIA_PLAZO hp
            WHERE
              hp.TZ_LOCK=0
              AND hp.TIPOMOV=&apos;R&apos;
              AND hp.RUBROCONTABLE LIKE &apos;14_5%&apos;
              AND hp.SALDOS_JTS_OID=s.JTS_OID
              AND ROWNUM&lt;2
            GROUP BY hp.SALDOS_JTS_OID
            &#47;*SELECT MINFECHAVALOR 
             FROM STG.T_BS_HISTORIA_PLAZO_SUMMARY2 BSHPSUMMARY2 
             WHERE BSHPSUMMARY2.SALDOS_JTS_OID=s.JTS_OID*&#47;
       ) 
         ELSE (s.C1628+(SELECT cc2.INCUMPLIMIENTOS FROM EDYFICAR.CR_CATEGORIAS_CARTERA cc2 WHERE cc2.CODIGO_ATRASO_DESTINO = &apos;N&apos; AND cc2.RUBRO_ORIGEN = s.C1692 AND ROWNUM&lt;2))
      END
    END) AS FE_PSJE_VENC
FROM EDYFICAR.SALDOS s
WHERE 
  s.TZ_LOCK = 0
AND   s.C9314 = 5</sql>
