  <step>
    <name>Leo EDYFICAR.SL_SOLICITUDCREDITO</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>TopazRepDS</connection>
    <sql>SELECT 
    s.C5000 AS C5000,
    s.GRUPO	AS GRUPO,
    s.C5061 AS C5061,
    s.C5004 AS C5004,
    s.C5074 AS C5074,
    s.C5044 AS C5044,
    s.C5261 AS C5261,
    CASE WHEN ( g.C1371 IS NOT NULL ) THEN 1
   		 ELSE 0
    END AS IN_GRUPAL,
	FCREAC1ERET,
	s.C5062 AS C5062,
	s.C5036 AS C5036,
	s.C5035 AS C5035,
	s.C5008 AS C5008,
	s.MONEDA AS MONEDA,
	s.C2320 AS C2320,
	s.C5001 AS C5001,
	s.C5006 AS C5006,
	s.C5187 AS C5187,
	s.C5063 AS C5063,
    s.C5002 AS C5002,
    s.C5053 AS C5053,
    s.FORMADESEMBOLSO AS FORMADESEMBOLSO,
    s.MB_DESEMBOLSO_JTS_OID AS MB_DESEMBOLSO_JTS_OID,
    s.TASA_OPTIMA AS TASA_OPTIMA,
    s.SEGURO AS SEGURO,
    s.VALOR_SEGURO AS VALOR_SEGURO,
    NVL((SELECT sr.COD_INV FROM EDYFICAR.SOL_REL_SUB_CRE sr WHERE sr.NUM_SOL = s.C5000),0) AS COD_INV,
    s.IDMETODOLOGIA AS IDMETODOLOGIA,
    s.TIPSOL AS TIPSOL,
    s.DIA_PAGO AS DIA_PAGO,
    s.C5229 AS C5229,
    s.C5054 AS C5054,
    s.C5023 AS C5023,
    s.C5223 AS C5223,
    s.C5024 AS C5024,
    s.TASA_MINIMA AS TASA_MINIMA,
    s.TASA_MAXIMA AS TASA_MAXIMA,
    s.C5281 AS C5281,
    s.C5039 AS C5039,
    s.FLG_CON AS FLG_CON,
    NVL((SELECT c.RESULTADO FROM EDYFICAR.SL_CRITERIOS c WHERE c.SOLICITUD = s.C5000 AND c.CODITEM = 2 AND c.TZ_LOCK = 0),0) AS VL_ENDEUDAMIENTO,
    DECODE(s.C5063,12,(SELECT a.TIPONOTA FROM EDYFICAR.SL_NOTASPORSOL a WHERE a.ORDINAL = (SELECT MAX(ORDINAL) FROM EDYFICAR.SL_NOTASPORSOL b WHERE b.NROSOLICITUD = a.NROSOLICITUD) AND a.NROSOLICITUD = s.C5000)) AS CO_MOTI_RECH,
    DECODE(s.C5063,12,(SELECT a.COMENTARIO FROM EDYFICAR.SL_NOTASPORSOL a WHERE a.ORDINAL = (SELECT MAX(ORDINAL) FROM EDYFICAR.SL_NOTASPORSOL b WHERE b.NROSOLICITUD = a.NROSOLICITUD) AND a.NROSOLICITUD = s.C5000),&apos; &apos;) AS DE_COME_RECH,
    NVL((SELECT sd.COD_CIA_SEG FROM EDYFICAR.SL_SEGURODESGRAVAMEN sd JOIN EDYFICAR.SL_SOLICITUDCREDITOPERSONA sp ON sd.NUM_SOLICITUD = sp.C5080 AND sd.TIPO_DOC = sp.C5081 AND sd.NRO_DOC = sp.C5082 AND sd.PAIS = sp.C5083 WHERE sd.NUM_SOLICITUD = s.C5000 AND sp.C5084 = &apos;T&apos;),0) AS CO_COMP_ASEG,
    NVL((SELECT sd.SEG_EXT FROM EDYFICAR.SL_SEGURODESGRAVAMEN sd JOIN EDYFICAR.SL_SOLICITUDCREDITOPERSONA sp ON sd.NUM_SOLICITUD = sp.C5080 AND sd.TIPO_DOC = sp.C5081 AND sd.NRO_DOC = sp.C5082 AND sd.PAIS = sp.C5083 WHERE sd.NUM_SOLICITUD = s.C5000 AND sp.C5084 = &apos;T&apos;),&apos; &apos;) AS IN_SEGU_DESG_EXTE,
    NVL((SELECT ((SELECT nvl(spc.DATO,&apos; &apos;) FROM EDYFICAR.SL_PROP_CREDITO spc WHERE spc.SOLICITUD = s.C5000 AND spc.ITEM = 17 AND spc.TZ_LOCK = 0)||&apos; &apos;||(SELECT nvl(cpc.DES_CRE_APO_CLI,&apos; &apos;) FROM EDYFICAR.CRE_PRO_CRE_RIE cpc WHERE cpc.NUM_SOL = s.C5000 AND cpc.TZ_LOCK = 0)) FROM DUAL),&apos; &apos;) AS DE_PROP_DEST,
	s.C5060 AS C5060
FROM EDYFICAR.SL_SOLICITUDCREDITO s
	 LEFT JOIN EDYFICAR.CL_GRUPOSSOCIALES g
	 	ON s.GRUPO=g.C1371
		 	AND g.TZ_LOCK=0
WHERE 
		s.TZ_LOCK=0
		AND s.C5060 &lt;= TO_DATE(&apos;29&#47;06&#47;2017&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)
		--AND s.C5060 &lt;= SYSDATE-1
</sql>
