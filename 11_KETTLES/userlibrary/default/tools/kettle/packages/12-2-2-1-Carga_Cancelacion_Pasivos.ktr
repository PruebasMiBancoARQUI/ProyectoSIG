    <connection>OdsStgDS</connection>
    <sql>--Caso Producto DPF
SELECT 
    EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100+EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) AS NU_PERI_MES,
    TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) AS FE_PROCESO,
    s.SUCURSAL AS CO_SUCURSAL,
    s.MONEDA AS CO_MONEDA,
    s.PRODUCTO AS CO_PRODUCTO,
    s.CUENTA AS NU_CUENTA,
    s.C1803 AS CO_CLIENTE,
    s.OPERACION AS NU_OPERACION,
    s.ORDINAL AS NU_ORDINAL,
    s.C1670 AS CO_ANALISTA,
    (SELECT c.VENCIMIENTO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS FE_CANCELACION,
    NVL((SELECT c.MODALIDA_PAGO_VENCIMIENTO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL),&apos; &apos;) TI_CANCELACION,
    (SELECT c.SUCURSALMOV FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS CO_SUCU_CANC,
    (SELECT a.INIUSR FROM STG.T_ASIENTOS_06 a JOIN STG.T_BS_HISTORIA_PLAZO_DPF hp ON a.ASIENTO = hp.NROASIENTOMOV AND hp.TIPOMOV=&apos;P&apos; AND a.SUCURSAL = hp.SUCURSALMOV AND a.FECHAPROCESO = hp.FECHAPROCESOMOV WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND   hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND ROWNUM=1) AS CO_ANAL_CANC,
    (SELECT a.HORAINICIO FROM STG.T_ASIENTOS_06 a JOIN STG.T_BS_HISTORIA_PLAZO_DPF hp ON a.ASIENTO = hp.NROASIENTOMOV AND hp.TIPOMOV=&apos;P&apos; AND a.SUCURSAL = hp.SUCURSALMOV AND a.FECHAPROCESO = hp.FECHAPROCESOMOV WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND   hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND ROWNUM=1) AS TX_HORA_CANC,
    (SELECT c.MODALIDA_PAGO_VENCIMIENTO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS NO_MODA_CANC,
    s.JTS_OID AS IN_SALD_JTS_OID,
    m.C6440 AS VL_TIPO_CAMB_OFIC,
    (SELECT c.TASA FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS VL_TASA_INTE_CANC, 
    (SELECT (NVL(hp.CAPITALPAGADO,0) + NVL(hp.INTERESPAGADO,0))*m.C6440 FROM STG.T_BS_HISTORIA_PLAZO_DPF hp WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND hp.TIPOMOV=&apos;P&apos; AND ROWNUM=1) AS MO_CANCE_SOLES,
    (SELECT c.USUARIO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS CO_USUA_TOPA, 
    (SELECT a.OPERACION FROM STG.T_ASIENTOS_06 a JOIN STG.T_BS_HISTORIA_PLAZO_DPF hp ON a.ASIENTO = hp.NROASIENTOMOV AND hp.TIPOMOV=&apos;P&apos; AND a.SUCURSAL = hp.SUCURSALMOV AND a.FECHAPROCESO = hp.FECHAPROCESOMOV WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND   hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND ROWNUM=1) AS CO_OPERATIVA,
    (SELECT a.HORAFIN FROM STG.T_ASIENTOS_06 a JOIN STG.T_BS_HISTORIA_PLAZO_DPF hp ON a.ASIENTO = hp.NROASIENTOMOV AND hp.TIPOMOV=&apos;P&apos; AND a.SUCURSAL = hp.SUCURSALMOV AND a.FECHAPROCESO = hp.FECHAPROCESOMOV WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND   hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND ROWNUM=1) AS TX_HORA_TERM_TRAN,
    (SELECT a.HORAINICIO FROM STG.T_ASIENTOS_06 a JOIN STG.T_BS_HISTORIA_PLAZO_DPF hp ON a.ASIENTO = hp.NROASIENTOMOV AND hp.TIPOMOV=&apos;P&apos; AND a.SUCURSAL = hp.SUCURSALMOV AND a.FECHAPROCESO = hp.FECHAPROCESOMOV WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND   hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND ROWNUM=1) AS TX_HORA_INIC_TRAN,
    (SELECT hp.CAPITALPAGADO FROM STG.T_BS_HISTORIA_PLAZO_DPF hp WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND hp.TIPOMOV=&apos;P&apos; AND ROWNUM=1) AS MO_CAPI_CANCE,
    (SELECT hp.INTERESPAGADO FROM STG.T_BS_HISTORIA_PLAZO_DPF hp WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND hp.TIPOMOV=&apos;P&apos; AND ROWNUM=1) AS MO_INTE_CANCE,
    (SELECT c.ITF_CANCELACION FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS MO_ITF_CANCEL,
    (SELECT NVL(hp.CAPITALPAGADO,0) + NVL(hp.INTERESPAGADO,0) FROM STG.T_BS_HISTORIA_PLAZO_DPF hp WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND hp.TIPOMOV=&apos;P&apos; AND ROWNUM=1) AS MO_CANCELACION,
    SYSDATE AS FE_ACTU_DIA
FROM STG.T_SALDOS_02 s
   JOIN STG.T_CO_MONEDAS_01 m
     ON s.MONEDA=m.C6399
   JOIN STG.T_BS_HISTORIA_PLAZO_SUMMARY hps
     ON hps.SALDOS_JTS_OID = s.JTS_OID
WHERE
        s.C9314 = 4 
        AND s.C1604 = 0


UNION


--Caso producto Ahorro, CTS y Cuenta Corriente
SELECT 
    EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100+EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) AS NU_PERI_MES,
    TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) AS FE_PROCESO,
    s.SUCURSAL AS CO_SUCURSAL,
    s.MONEDA AS CO_MONEDA,
    s.PRODUCTO AS CO_PRODUCTO,
    s.CUENTA AS NU_CUENTA,
    s.C1803 AS CO_CLIENTE,
    s.OPERACION AS NU_OPERACION,
    s.ORDINAL AS NU_ORDINAL,
    s.C1670 AS CO_ANALISTA,
    (SELECT c.FECHA FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS FE_CANCELACION,   
    (SELECT c.DESC_MOTIVO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS TI_CANCELACION,
    (SELECT c.SUCURSALMOV FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS CO_SUCU_CANC,   
    (SELECT a.INIUSR FROM STG.T_ASIENTOS_06 a JOIN STG.T_HISTORIA_VISTA_01 hv ON a.ASIENTO = hv.ASIENTO AND a.SUCURSAL = hv.SUCURSAL AND a.FECHAPROCESO = hv.FECHA_PROCESADO AND hv.EVENTO=&apos;M&apos; WHERE hv.SALDO_JTS_OID = hvs.SALDO_JTS_OID AND hv.FECHA_PROCESADO = hvs.FECHA_PROCESADO  AND hv.FECHA_VALOR = hvs.FECHA_VALOR AND ROWNUM=1) AS CO_ANAL_CANC,
    (SELECT a.HORAINICIO FROM STG.T_ASIENTOS_06 a JOIN STG.T_HISTORIA_VISTA_01 hv ON a.ASIENTO = hv.ASIENTO AND a.SUCURSAL = hv.SUCURSAL AND a.FECHAPROCESO = hv.FECHA_PROCESADO AND hv.EVENTO=&apos;M&apos; WHERE hv.SALDO_JTS_OID = hvs.SALDO_JTS_OID AND hv.FECHA_PROCESADO = hvs.FECHA_PROCESADO  AND hv.FECHA_VALOR = hvs.FECHA_VALOR AND ROWNUM=1) AS TX_HORA_CANC, 
     (SELECT c.MODALIDA_PAGO_VENCIMIENTO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS NO_MODA_CANC,
     s.JTS_OID AS IN_SALD_JTS_OID,
     m.C6440 AS VL_TIPO_CAMB_OFIC,
     0 AS VL_TASA_INTE_CANC,     
     0 MO_CANCE_SOLES,   
    (SELECT c.USUARIO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ) AS CO_USUA_TOPA,
    (SELECT a.OPERACION FROM STG.T_ASIENTOS_06 a JOIN STG.T_HISTORIA_VISTA_01 hv ON a.ASIENTO = hv.ASIENTO AND a.SUCURSAL = hv.SUCURSAL AND a.FECHAPROCESO = hv.FECHA_PROCESADO AND hv.EVENTO=&apos;M&apos; WHERE hv.SALDO_JTS_OID = hvs.SALDO_JTS_OID AND hv.FECHA_PROCESADO = hvs.FECHA_PROCESADO  AND hv.FECHA_VALOR = hvs.FECHA_VALOR AND ROWNUM=1) AS CO_OPERATIVA,
    (SELECT a.HORAFIN FROM STG.T_ASIENTOS_06 a JOIN STG.T_HISTORIA_VISTA_01 hv ON a.ASIENTO = hv.ASIENTO AND a.SUCURSAL = hv.SUCURSAL AND a.FECHAPROCESO = hv.FECHA_PROCESADO AND hv.EVENTO=&apos;M&apos; WHERE hv.SALDO_JTS_OID = hvs.SALDO_JTS_OID AND hv.FECHA_PROCESADO = hvs.FECHA_PROCESADO  AND hv.FECHA_VALOR = hvs.FECHA_VALOR AND ROWNUM=1) AS TX_HORA_TERM_TRAN,
    (SELECT a.HORAINICIO FROM STG.T_ASIENTOS_06 a JOIN STG.T_HISTORIA_VISTA_01 hv ON a.ASIENTO = hv.ASIENTO AND a.SUCURSAL = hv.SUCURSAL AND a.FECHAPROCESO = hv.FECHA_PROCESADO AND hv.EVENTO=&apos;M&apos; WHERE hv.SALDO_JTS_OID = hvs.SALDO_JTS_OID AND hv.FECHA_PROCESADO = hvs.FECHA_PROCESADO  AND hv.FECHA_VALOR = hvs.FECHA_VALOR AND ROWNUM=1) AS TX_HORA_INIC_TRAN,
     0 AS MO_CAPI_CANCE,
     0 AS MO_INTE_CANCE,
     0 AS MO_ITF_CANCEL,
     0 MO_CANCELACION,
     SYSDATE AS FE_ACTU_DIA
FROM STG.T_SALDOS_02 s
   JOIN STG.T_CO_MONEDAS_01 m
     ON s.MONEDA=m.C6399
   LEFT JOIN STG.T_HISTORIA_VISTA_SUMMARY hvs
        ON hvs.SALDO_JTS_OID = s.JTS_OID
WHERE
