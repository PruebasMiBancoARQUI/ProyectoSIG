    <connection>OdsStgDS</connection>
    <sql>--@001 CHC CASO-21672 [Proyecto SIG] Despliegue de BDS COBRANZAS.
WITH SUBQ1 AS(
              SELECT 
                    SOL.C5061, SOL.C5062, SOL.C5000
              FROM  
                    STG.T_SL_SOLICITUDCREDCANCELA SCC 
                                  JOIN STG.T_SALDOS s2 
                                        ON SCC.CRE_SUCURSAL = s2.SUCURSAL 
                                            AND SCC.CRE_PRODUCTO = s2.PRODUCTO 
                                            AND SCC.CRE_CUENTA = s2.CUENTA 
                                            AND SCC.CRE_MONEDA = s2.MONEDA 
                                            AND SCC.CRE_OPERACION = s2.OPERACION 
                                            AND SCC.CRE_ORDINAL = s2.ORDINAL 
                                  JOIN STG.T_SL_SOLICITUDCREDITO SOL 
                                        ON SOL.C5000 = s2.C1704
              GROUP BY SOL.C5061, SOL.C5062, SOL.C5000                                                                   
),
SUBQ2 AS(
             SELECT 
                    SCC.CRE_PRODUCTO, 
                    SCC.CRE_NUMSOLICITUD,
                    SCC.CRE_MONTOCANC
             FROM 
                    STG.T_SL_SOLICITUDCREDCANCELA SCC 
             WHERE  
                    SCC.CRE_MOTIVOCANC = &apos;P&apos;
					AND NOT EXISTS (SELECT 1 
                       				FROM STG.T_SL_SOLICITUDCREDCANCELA SCC4 
                       				WHERE SCC.CRE_NUMSOLICITUD=SCC4.CRE_NUMSOLICITUD 
                             				AND SCC4.CRE_MOTIVOCANC = &apos;P&apos;
                             				AND SCC4.CRE_MONTOCANC&gt;SCC.CRE_MONTOCANC)
),
SUBQ3 AS(
			SELECT 
       				SCC2.CRE_PRODUCTO, 
       				SCC2.CRE_NUMSOLICITUD,
       				SCC2.CRE_MONTOCANC                     
			FROM   
       				STG.T_SL_SOLICITUDCREDCANCELA SCC2                                                  
			WHERE 
       				SCC2.CRE_MOTIVOCANC = &apos;R&apos;
       				AND NOT EXISTS (SELECT 1 
                       				FROM STG.T_SL_SOLICITUDCREDCANCELA SCC3 
                       				WHERE SCC2.CRE_NUMSOLICITUD=SCC3.CRE_NUMSOLICITUD 
                             				AND SCC3.CRE_MOTIVOCANC = &apos;R&apos;
                             				AND SCC3.CRE_MONTOCANC&gt;SCC2.CRE_MONTOCANC)
)

SELECT
        SALDOS.CUENTA,          
        SALDOS.MONEDA,          
        SALDOS.SUCURSAL,        
        SALDOS.PRODUCTO,        
        SALDOS.OPERACION,        
        SALDOS.ORDINAL,          
        SALDOS.JTS_OID,          
        SALDOS.C1803,          
        SALDOS.C1621,          
        SALDOS.C1627,          
        SALDOS.C1642,          
        SALDOS.C1644,          
        SALDOS.C1632,          
        SALDOS.C1728,          
        SALDOS.C1628,          
        SALDOS.C1601,          
        SALDOS.C1604,          
        SALDOS.SALDOMN,          
        SALDOS.C1661,          
        SALDOS.REFINANCIACION,      
        SALDOS.C1670,          
        SALDOS.CRITERIO,        
        SALDOS.C1711,          
        SALDOS.ENTREGA_ACUENTA_ICMORA,
        SALDOS.TASA_ICMORA,
        SALDOS.C1633,      
        SALDOS.C1659,          
        SALDOS.C1679,          
        SALDOS.C1730,          
        SALDOS.C1609,          
        SALDOS.C1610,          
        SALDOS.C9661,          
        SALDOS.C6645,          
        SALDOS.C1704,          
        SALDOS.C9314,          
        SALDOS.C1705,          
        SALDOS.C1645,          
        SALDOS.FECCAST,          
        SALDOS.C1608,          
        SALDOS.C2294,
        SALDOS.C4280,
        SALDOS.MB_REFERENCIA_GRUPAL,
        SALDOS.USUTOPAZ,
        SALDOS.C1612,
        SALDOS.MB_CREDITO_MIGRADO,
        SALDOS.REFINANCIADO,
        SALDOS.C1692,
        SOLCRED.C5000,                      
        SOLCRED.GRUPO,    
        SOLCRED.C5061,    
        SOLCRED.C5004,    
        SOLCRED.C5074,    
        SOLCRED.C5044,    
        SOLCRED.C5261,    
        SOLCRED.IN_GRUPAL,  
        SOLCRED.FCREAC1ERET,  
        SOLCRED.C5062,    
        SOLCRED.C5036,    
        SOLCRED.C5035,    
        SOLCRED.C5008,        
        SOLCRED.C2320,    
        SOLCRED.C5001,
        SOLCRED.C5006,
        SOLCRED.FORMADESEMBOLSO,
        SOLCRED.MB_DESEMBOLSO_JTS_OID,
        SOLCRED.TASA_OPTIMA,
        SOLCRED.SEGURO,
        SOLCRED.VALOR_SEGURO,
        SOLCRED.COD_INV,
        SOLCRED.CO_COMP_ASEG,
        SOLCRED.IN_SEGU_DESG_EXTE,
        (CASE WHEN SOLCRED.C5004 = &apos;PRESTAMO PRE-PAGO&apos;
                    THEN  SUBQ1.C5061
              WHEN SALDOS.C1661 &gt; 0 
                    THEN SOLCRED.C5061 -- Caso de disposiciones
              ELSE SOLCRED.C5061
        END ) AS USU_EVAL,
        (CASE WHEN SOLCRED.C5004 = &apos;PRESTAMO PRE-PAGO&apos; 
                    THEN SUBQ1.C5062
              WHEN SALDOS.C1661 &gt; 0 
                    THEN SOLCRED.C5006 -- Caso de disposiciones
              ELSE SOLCRED.C5062
        END ) AS FECHA_EVAL,
        (CASE WHEN SOLCRED.C5004 = &apos;PRESTAMO PRE-PAGO&apos; 
                    THEN SUBQ1.C5061
              WHEN SALDOS.C1661 &gt; 0 
                    THEN (SELECT SOL.C5061 
                          FROM STG.T_CR_LINEACREDITO LINEA 
                                        JOIN STG.T_SL_SOLICITUDCREDITO SOL 
                                            ON LINEA.NROSOL = SOL.C5000 
                          WHERE LINEA.NROLINEA = SALDOS.C1661) -- Caso de disposiciones
              ELSE SOLCRED.C5061
        END ) AS USU_ORIGINAL,
        (CASE 
              WHEN SOLCRED.C5004 = &apos;PRESTAMO PRE-PAGO&apos; 
                    THEN SUBQ1.C5062
              WHEN SALDOS.C1661 &gt; 0 
                    THEN (SELECT SOL.C5062 
                          FROM STG.T_CR_LINEACREDITO LINEA 
                                          JOIN STG.T_SL_SOLICITUDCREDITO SOL 
                                            ON LINEA.NROSOL = SOL.C5000 
                          WHERE LINEA.NROLINEA = SALDOS.C1661) -- Caso de disposiciones
              ELSE SOLCRED.C5062
        END ) AS FECHA_ORIGINAL,
   		SOLCRED.TASA_MINIMA AS TASA_MINIMA,
   		SOLCRED.TASA_MAXIMA AS TASA_MAXIMA,
        (CASE 
              WHEN SOLCRED.C5004 = &apos;PRESTAMO PRE-PAGO&apos; 
                    THEN SUBQ2.CRE_PRODUCTO
              WHEN SOLCRED.C5004 = &apos;REFINANCIADO&apos;   
                    THEN SUBQ3.CRE_PRODUCTO-- Caso de prestamos Refinanciados
          ELSE 0
         END) AS CO_PROD_ORIG,
        NVL(a.OPERACION,0) AS CO_OPERATIVA
--@001 INICIO
        ,SALDOS.INTE_COMP AS INTE_COMP
        ,SALDOS.MORA_CONT AS MORA_CONT
--@001 FINA
FROM  
        STG.T_SALDOS SALDOS
                        JOIN STG.T_SL_SOLICITUDCREDITO SOLCRED
                                  ON SALDOS.C1704=SOLCRED.C5000
                        LEFT JOIN STG.T_BS_HISTORIA_PLAZO_SLCRED hp
                                  ON hp.SALDOS_JTS_OID = SALDOS.JTS_OID
                                    AND hp.TIPOMOV=&apos;A&apos;
                        LEFT JOIN STG.T_ASIENTOS_06 a 
                                  ON a.ASIENTO = hp.NROASIENTOMOV 
                                    AND a.SUCURSAL = hp.SUCURSALMOV 
                                    AND a.FECHAPROCESO = hp.FECHAPROCESOMOV
                        LEFT JOIN SUBQ1
                                  ON SUBQ1.C5000=SALDOS.C1704
                        LEFT JOIN SUBQ2
                                  ON SUBQ2.CRE_NUMSOLICITUD=SALDOS.C1704
                        LEFT JOIN SUBQ3
                                  ON SUBQ3.CRE_NUMSOLICITUD=SALDOS.C1704                                  
WHERE 
        SALDOS.C9314=5
        AND SALDOS.C1604&lt;0</sql>
