  <step>
    <name>Leo T_SALDOS y T_SL_SOLICITUDCREDITO</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
