    <connection>OdsStgDS</connection>
    <sql>SELECT CASE 
         WHEN TO_CHAR(A.FE_PROCESO,&apos;YYYYMM&apos;) = &apos;${NU_PERI_PROC}&apos; OR TO_CHAR(A.FE_CONTABLE,&apos;YYYYMM&apos;) = &apos;${NU_PERI_PROC}&apos; THEN TO_NUMBER(&apos;${NU_PERI_PROC}&apos;) 
         ELSE 0 
       END NU_PERI_MES,
       A.CO_CLIENTE, 
       A.NU_CUENTA, 
       A.CO_MONEDA, 
       A.CO_PRODUCTO, 
       A.ID_CUEN_CONT NU_CUEN_CONT, 
       A.FE_PROCESO FE_MOVIMIENTO, 
       A.FE_VALOR, 
       A.CO_OFICINA CO_SUCURSAL, 
       A.CO_OFIC_ORIG CO_SUCU_ORIG, 
       A.NU_ASIENTO, 
       CASE WHEN REGEXP_INSTR(A.NO_ASIENTO, &apos;[^0-9]&apos;)&gt;0 THEN A.NO_ASIENTO ELSE A.DE_ASIENTO END DE_ASIENTO, 
       A.NU_TRREAL, 
       A.NU_ORDINAL, 
       A.NU_TRVIRTUAL, 
       A.NU_TRTIPO, 
       A.TI_MOVI_CONTA, 
       A.IN_SIGNO                           TI_SIGNO, 
       A.MO_IMPORTE, 
       A.MO_IMPO_MN, 
       A.CO_OPERACION CO_OPERATIVA, 
       A.IN_AJUSTE                          TI_AJUSTE, 
       A.DE_REFE_ASIE,
       NVL(REGEXP_REPLACE(CASE WHEN REGEXP_INSTR(A.NO_ASIENTO, &apos;\d&apos;)&gt;0 THEN A.NO_ASIENTO ELSE A.DE_ASIENTO END , &apos;[^0-9]&apos;),0) NU_CUEN_CLIE
FROM   ODS.HD_ASIENTOS A
WHERE  ((A.FE_PROCESO &gt;= TRUNC(ADD_MONTHS(TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;, &apos;DD&#47;MM&#47;YYYY&apos;),-1), &apos;MM&apos;) AND A.IN_AJUSTE  = &apos; &apos;)
         OR (A.FE_CONTABLE &gt;= TRUNC(ADD_MONTHS(TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;, &apos;DD&#47;MM&#47;YYYY&apos;),-1), &apos;MM&apos;) AND  A.IN_AJUSTE   = &apos;A&apos;))
       AND (A.ID_CUEN_CONT LIKE &apos;51%&apos; OR A.ID_CUEN_CONT LIKE &apos;52%&apos; OR A.ID_CUEN_CONT LIKE &apos;41%&apos; OR A.ID_CUEN_CONT LIKE &apos;42%&apos;)
 
</sql>
