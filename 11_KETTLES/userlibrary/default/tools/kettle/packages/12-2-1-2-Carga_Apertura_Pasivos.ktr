    <connection>OdsStgDS</connection>
    <sql>--Caso Producto DPF
SELECT 
  EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100+EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) AS NU_PERI_MES,
  TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) AS FE_PROCESO,
  s.SUCURSAL AS CO_SUCURSAL,
  s.MONEDA AS CO_MONEDA,
  s.PRODUCTO AS CO_PRODUCTO,
  s.CUENTA AS NU_CUENTA,
  s.C1803 AS CO_CLIENTE,
  s.OPERACION AS NU_OPERACION,
  s.ORDINAL AS NU_ORDINAL,  
  s.C1601 AS MO_APERTURA,
  CAST(0 AS NUMBER(15,2)) AS MO_CANCELACION,
  NVL((s.C1627 -s.C1621),0) AS NU_PLAZO,
  s.C1627 AS FE_VENCIMIENTO,
  s.C1632 AS VL_TASA_INTE,
  s.C1659 AS CO_TIPO_RENO,
  s.C1670 AS CO_ANALISTA,
 (SELECT c.MEDIOORIGEN FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ) AS NO_MEDI_ORIG,
 (SELECT c.PAGO_APERTURA FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ) AS TI_PAGO_APER,
 (SELECT c.ITF_APERTURA FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ) AS MO_ITF_APER,
 (SELECT c.ORIGENLLEGADA FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ) AS NO_ORIG_LLEG,
 s.C1621 AS FE_APERTURA,
 CAST(&apos;&apos; AS DATE) AS FE_CANCELACION,
  CASE WHEN (s.C1627 - s.C1621) - (TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) - s.C1621) &gt; 360 THEN &apos;L&apos;
     ELSE &apos;C&apos;
  END AS CO_TIPO_PLAZ,
  s.C1730 AS CO_CUEN_CONT,
  s.C1705 AS CO_PIZARRA,
  m.C6440 AS VL_TIPO_CAMB_OFIC,
  s.JTS_OID AS IN_SALD_JTS_OID,
  (SELECT CLIENTES.C1058 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTI_ECON_INTE,
  (SELECT CLIENTES.C3205 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTIVIDAD,
  (SELECT CLIENTES.C1034 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTI_CIIU, 
  NVL((s.C1601*C6440),0) AS MO_APER_SOLES,
  (SELECT PROD.C6252 FROM STG.T_CO_PRODUCTOS PROD WHERE PROD.C6250=s.PRODUCTO AND PROD.C6252 = s.C9314) AS CO_TIPO_PASI,
  0 AS CO_TIPO_AHOR,
  NVL((SELECT c.USUARIO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ),&apos; &apos;) AS CO_USUA_TOPA,
  (SELECT a.HORAFIN FROM STG.T_ASIENTOS_06 a WHERE a.ASIENTO = hp.NROASIENTOMOV AND a.SUCURSAL = hp.SUCURSALMOV AND a.FECHAPROCESO = hp.FECHAPROCESOMOV) AS TX_HORA_TERM_TRAN,
  (SELECT a.HORAINICIO FROM STG.T_ASIENTOS_06 a WHERE a.ASIENTO = hp.NROASIENTOMOV AND a.SUCURSAL = hp.SUCURSALMOV AND a.FECHAPROCESO = hp.FECHAPROCESOMOV) AS TX_HORA_INIC_TRAN,
  NVL((SELECT a.OPERACION FROM STG.T_ASIENTOS_06 a WHERE a.ASIENTO = hp.NROASIENTOMOV AND a.SUCURSAL = hp.SUCURSALMOV AND a.FECHAPROCESO = hp.FECHAPROCESOMOV),0) AS CO_OPERATIVA,
  &apos;A&apos; AS TI_VIGENCIA,
  SYSDATE AS FE_ACTU_DIA
FROM STG.T_SALDOS_02 s
   JOIN STG.T_CO_MONEDAS_01 m
     ON s.MONEDA=m.C6399
   LEFT JOIN STG.T_BS_HISTORIA_PLAZO_DPF hp 
        ON hp.SALDOS_JTS_OID=s.JTS_OID
            AND hp.TIPOMOV=&apos;A&apos; 
WHERE
    s.C9314 = 4 
AND s.ORDINAL=0 --AND s.C1604&lt;&gt;0

UNION ALL
SELECT 
  EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100+EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) AS NU_PERI_MES,
  TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) AS FE_PROCESO,
  s.SUCURSAL AS CO_SUCURSAL,
  s.MONEDA AS CO_MONEDA,
  s.PRODUCTO AS CO_PRODUCTO,
  s.CUENTA AS NU_CUENTA,
  s.C1803 AS CO_CLIENTE,
  s.OPERACION AS NU_OPERACION,
  s.ORDINAL AS NU_ORDINAL,  
  CAST(0 AS NUMBER(15,2)) AS MO_APERTURA,
  (SELECT NVL(hp.CAPITALPAGADO,0) + NVL(hp.INTERESPAGADO,0) FROM STG.T_BS_HISTORIA_PLAZO_DPF hp WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND hp.TIPOMOV=&apos;P&apos; AND ROWNUM=1) AS MO_CANCELACION,
  NVL((s.C1627 -s.C1621),0) AS NU_PLAZO,
  s.C1627 AS FE_VENCIMIENTO,
  s.C1632 AS VL_TASA_INTE,
  s.C1659 AS CO_TIPO_RENO,
  s.C1670 AS CO_ANALISTA,
 (SELECT c.MEDIOORIGEN FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ) AS NO_MEDI_ORIG,
 (SELECT c.PAGO_APERTURA FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ) AS TI_PAGO_APER,
 (SELECT c.ITF_APERTURA FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ) AS MO_ITF_APER,
 (SELECT c.ORIGENLLEGADA FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ) AS NO_ORIG_LLEG,
  s.C1621 AS FE_APERTURA,
 (SELECT c.VENCIMIENTO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS FE_CANCELACION,
  CASE WHEN (s.C1627 - s.C1621) - (TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) - s.C1621) &gt; 360 THEN &apos;L&apos;
     ELSE &apos;C&apos;
  END AS CO_TIPO_PLAZ,
  s.C1730 AS CO_CUEN_CONT,
  s.C1705 AS CO_PIZARRA,
  m.C6440 AS VL_TIPO_CAMB_OFIC,
  s.JTS_OID AS IN_SALD_JTS_OID,
  (SELECT CLIENTES.C1058 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTI_ECON_INTE,
  (SELECT CLIENTES.C3205 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTIVIDAD,
  (SELECT CLIENTES.C1034 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTI_CIIU, 
  NVL((s.C1601*C6440),0) AS MO_APER_SOLES,
  (SELECT PROD.C6252 FROM STG.T_CO_PRODUCTOS PROD WHERE PROD.C6250=s.PRODUCTO AND PROD.C6252 = s.C9314) AS CO_TIPO_PASI,
  0 AS CO_TIPO_AHOR,
  NVL((SELECT c.USUARIO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ),&apos; &apos;) AS CO_USUA_TOPA,
  (SELECT a.HORAFIN FROM STG.T_ASIENTOS_06 a JOIN STG.T_BS_HISTORIA_PLAZO_DPF hp ON a.ASIENTO = hp.NROASIENTOMOV AND hp.TIPOMOV=&apos;P&apos; AND a.SUCURSAL = hp.SUCURSALMOV AND a.FECHAPROCESO = hp.FECHAPROCESOMOV WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND ROWNUM=1) AS TX_HORA_TERM_TRAN,
  (SELECT a.HORAINICIO FROM STG.T_ASIENTOS_06 a JOIN STG.T_BS_HISTORIA_PLAZO_DPF hp ON a.ASIENTO = hp.NROASIENTOMOV AND hp.TIPOMOV=&apos;P&apos; AND a.SUCURSAL = hp.SUCURSALMOV AND a.FECHAPROCESO = hp.FECHAPROCESOMOV WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND ROWNUM=1) AS TX_HORA_INIC_TRAN,
  NVL((SELECT a.OPERACION FROM STG.T_ASIENTOS_06 a JOIN STG.T_BS_HISTORIA_PLAZO_DPF hp ON a.ASIENTO = hp.NROASIENTOMOV AND hp.TIPOMOV=&apos;P&apos; AND a.SUCURSAL = hp.SUCURSALMOV AND a.FECHAPROCESO = hp.FECHAPROCESOMOV WHERE hp.SALDOS_JTS_OID = hps.SALDOS_JTS_OID AND hp.FECHAVALOR = hps.FECHAVALOR AND hp.FECHAPROCESOMOV = hps.FECHAPROCESOMOV AND ROWNUM=1),0) AS CO_OPERATIVA,
  &apos;C&apos; AS TI_VIGENCIA,
  SYSDATE AS FE_ACTU_DIA
FROM STG.T_SALDOS_02 s
   JOIN STG.T_CO_MONEDAS_01 m
     ON s.MONEDA=m.C6399
   LEFT JOIN STG.T_BS_HISTORIA_PLAZO_SUMMARY hps
        ON hps.SALDOS_JTS_OID=s.JTS_OID
            
WHERE
    s.C9314 = 4 
AND s.C1604 = 0 AND s.ORDINAL=0

UNION ALL
--Caso producto Ahorro, CTS y Cuenta Corriente
SELECT 
    EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100+EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) AS NU_PERI_MES,
    TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) AS FE_PROCESO,
    s.SUCURSAL AS CO_SUCURSAL,
    s.MONEDA AS CO_MONEDA,
    s.PRODUCTO AS CO_PRODUCTO,
    s.CUENTA AS NU_CUENTA,
    s.C1803 AS CO_CLIENTE,
    s.OPERACION AS NU_OPERACION,
    s.ORDINAL AS NU_ORDINAL,
    NVL(DECODE(s.C1601,0,s.C1604),0) AS MO_APERTURA,
    0 MO_CANCELACION, 
    NVL((s.C1627 -s.C1621),0) AS NU_PLAZO,
    s.C1627 AS FE_VENCIMIENTO,  
    s.C2294 AS VL_TASA_INTE,
    s.C1659 AS CO_TIPO_RENO,
    s.C1670 AS CO_ANALISTA,
    NVL((SELECT c.MEDIOORIGEN FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ),&apos; &apos;) AS NO_MEDI_ORIG,
    &apos;0&apos; AS TI_PAGO_APER,
    0 AS MO_ITF_APER,
    NVL((SELECT c.ORIGENLLEGADA FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ),&apos; &apos;) AS NO_ORIG_LLEG,   
    s.C1621 AS FE_APERTURA,
    CAST(&apos;&apos; AS DATE) AS FE_CANCELACION,
    CASE WHEN (s.C1627 - TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) &gt; 360 THEN &apos;L&apos;
         ELSE &apos;C&apos;
    END AS CO_TIPO_PLAZ,
    s.C1730 AS CO_CUEN_CONT,
    s.C1705 AS CO_PIZARRA,
    m.C6440 AS VL_TIPO_CAMB_OFIC,
    s.JTS_OID AS IN_SALD_JTS_OID,
    (SELECT CLIENTES.C1058 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTI_ECON_INTE,
    (SELECT CLIENTES.C3205 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTIVIDAD,
    (SELECT CLIENTES.C1034 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTI_CIIU, 
    NVL((DECODE(s.C1601,0,s.C1604)*C6440),0) AS MO_APER_SOLES,
    (SELECT PROD.C6252 FROM STG.T_CO_PRODUCTOS PROD WHERE PROD.C6250=s.PRODUCTO AND PROD.C6252 = s.C9314) AS CO_TIPO_PASI,
    NVL((CASE WHEN s.C9314 = 3 THEN (SELECT PROD.C6734 FROM STG.T_CO_PRODUCTOS PROD WHERE PROD.C6250=s.PRODUCTO AND PROD.C6252 = s.C9314) 
         ELSE 0 
    END),0) AS CO_TIPO_AHOR,
    NVL((SELECT c.USUARIO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ),&apos; &apos;) AS CO_USUA_TOPA,
    (SELECT a.HORAFIN FROM STG.T_ASIENTOS_06 a WHERE a.ASIENTO = hv.ASIENTO AND a.SUCURSAL = hv.SUCURSAL AND a.FECHAPROCESO = hv.FECHA_PROCESADO) AS TX_HORA_TERM_TRAN,
    (SELECT a.HORAINICIO FROM STG.T_ASIENTOS_06 a WHERE a.ASIENTO = hv.ASIENTO AND a.SUCURSAL = hv.SUCURSAL AND a.FECHAPROCESO = hv.FECHA_PROCESADO) AS TX_HORA_INIC_TRAN,
    NVL((SELECT a.OPERACION FROM STG.T_ASIENTOS_06 a WHERE a.ASIENTO = hv.ASIENTO AND a.SUCURSAL = hv.SUCURSAL AND a.FECHAPROCESO = hv.FECHA_PROCESADO),0) AS CO_OPERATIVA,
    &apos;A&apos; AS TI_VIGENCIA,
    SYSDATE AS FE_ACTU_DIA
FROM STG.T_SALDOS_02 s
   JOIN STG.T_CO_MONEDAS_01 m
     ON s.MONEDA=m.C6399
   LEFT JOIN STG.T_HISTORIA_VISTA_01 hv
        ON hv.SALDO_JTS_OID = s.JTS_OID
            AND hv.EVENTO = &apos;A&apos;
WHERE
     s.C9314 IN (2,3) AND s.ORDINAL=0
    --AND EXISTS (SELECT 1 FROM STG.T_SALDOS_DIARIOS sd WHERE sd.SALDO_JTS_OID=s.JTS_OID)
    
UNION ALL
 
SELECT 
    EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100+EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) AS NU_PERI_MES,
    TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) AS FE_PROCESO,
    s.SUCURSAL AS CO_SUCURSAL,
    s.MONEDA AS CO_MONEDA,
    s.PRODUCTO AS CO_PRODUCTO,
    s.CUENTA AS NU_CUENTA,
    s.C1803 AS CO_CLIENTE,
    s.OPERACION AS NU_OPERACION,
    s.ORDINAL AS NU_ORDINAL,
    CAST(0 AS NUMBER(15,2)) AS MO_APERTURA,
    0 MO_CANCELACION, 
    NVL((s.C1627 -s.C1621),0) AS NU_PLAZO,
    s.C1627 AS FE_VENCIMIENTO,  
    s.C2294 AS VL_TASA_INTE,
    s.C1659 AS CO_TIPO_RENO,
    s.C1670 AS CO_ANALISTA,
    NVL((SELECT c.MEDIOORIGEN FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ),&apos; &apos;) AS NO_MEDI_ORIG,
    &apos;0&apos; AS TI_PAGO_APER,
    0 AS MO_ITF_APER,
    NVL((SELECT c.ORIGENLLEGADA FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ),&apos; &apos;) AS NO_ORIG_LLEG,
    s.C1621 AS FE_APERTURA,   
    (SELECT c.FECHA FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL) AS FE_CANCELACION,
    CASE WHEN (s.C1627 - TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) &gt; 360 THEN &apos;L&apos;
         ELSE &apos;C&apos;
    END AS CO_TIPO_PLAZ,
    s.C1730 AS CO_CUEN_CONT,
    s.C1705 AS CO_PIZARRA,
    m.C6440 AS VL_TIPO_CAMB_OFIC,
    s.JTS_OID AS IN_SALD_JTS_OID,
    (SELECT CLIENTES.C1058 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTI_ECON_INTE,
    (SELECT CLIENTES.C3205 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTIVIDAD,
    (SELECT CLIENTES.C1034 FROM STG.T_CL_CLIENTES_01 CLIENTES WHERE  CLIENTES.C0902=s.C1803)  AS CO_ACTI_CIIU, 
    NVL((DECODE(s.C1601,0,s.C1604)*C6440),0) AS MO_APER_SOLES,
    (SELECT PROD.C6252 FROM STG.T_CO_PRODUCTOS PROD WHERE PROD.C6250=s.PRODUCTO AND PROD.C6252 = s.C9314) AS CO_TIPO_PASI,
    NVL((CASE WHEN s.C9314 = 3 THEN (SELECT PROD.C6734 FROM STG.T_CO_PRODUCTOS PROD WHERE PROD.C6250=s.PRODUCTO AND PROD.C6252 = s.C9314) 
         ELSE 0 
    END),0) AS CO_TIPO_AHOR,
    NVL((SELECT c.USUARIO FROM STG.T_AP_CATALOGODPF c WHERE c.SUCURSAL=s.SUCURSAL AND c.PRODUCTO=s.PRODUCTO AND c.CUENTA=s.CUENTA AND c.MONEDA=s.MONEDA AND c.OPERACION=s.OPERACION AND c.ORDINAL=s.ORDINAL ),&apos; &apos;) AS CO_USUA_TOPA,
    (SELECT MAX(a.HORAFIN) FROM STG.T_ASIENTOS_06 a,STG.T_HISTORIA_VISTA_01 hv WHERE a.ASIENTO = hv.ASIENTO AND a.SUCURSAL = hv.SUCURSAL AND a.FECHAPROCESO = hv.FECHA_PROCESADO AND HV.SALDO_JTS_OID=HVS.SALDO_JTS_OID AND ROWNUM=1) AS TX_HORA_TERM_TRAN,
    (SELECT MAX(a.HORAINICIO) FROM STG.T_ASIENTOS_06 a,STG.T_HISTORIA_VISTA_01 hv WHERE a.ASIENTO = hv.ASIENTO AND a.SUCURSAL = hv.SUCURSAL AND a.FECHAPROCESO = hv.FECHA_PROCESADO AND HV.SALDO_JTS_OID=HVS.SALDO_JTS_OID AND ROWNUM=1) AS TX_HORA_INIC_TRAN,
    NVL((SELECT a.OPERACION FROM STG.T_ASIENTOS_06 a,STG.T_HISTORIA_VISTA_01 hv WHERE a.ASIENTO = hv.ASIENTO AND a.SUCURSAL = hv.SUCURSAL AND a.FECHAPROCESO = hv.FECHA_PROCESADO AND hv.FECHA_VALOR = hvs.FECHA_VALOR AND HV.SALDO_JTS_OID=HVS.SALDO_JTS_OID AND ROWNUM=1),0) AS CO_OPERATIVA,
    &apos;C&apos; AS TI_VIGENCIA,
    SYSDATE AS FE_ACTU_DIA
FROM STG.T_SALDOS_02 s
   JOIN STG.T_CO_MONEDAS_01 m
     ON s.MONEDA=m.C6399
   LEFT JOIN STG.T_HISTORIA_VISTA_SUMMARY hvs
        ON hvs.SALDO_JTS_OID = s.JTS_OID
            
WHERE
     s.C9314 IN (2,3) AND s.C1651=&apos;1&apos; AND s.ORDINAL=0
    </sql>
