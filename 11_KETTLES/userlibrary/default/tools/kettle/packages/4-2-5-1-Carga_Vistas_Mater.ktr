  <step>
    <name>Consulto datos Intereses</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>SELECT
S.CUENTA AS CUENTA,
  SUM(CASE WHEN S.C1728=&apos;N&apos; AND TO_NUMBER ( NVL (CALIFCLIENT.CALIFALIN,0)) &lt;= 2 
          THEN
              (CASE WHEN (S.C1609-(S.C1610+S.C9661)) &gt; 0  
                  THEN (S.C1609-(S.C1610+S.C9661))
                ELSE
                  0
              END
                )
        ELSE
          0
      END
    ) AS MO_INTE_DEVE_VIGE,
  SUM(CASE WHEN S.C1728!=&apos;N&apos; OR (S.C1728=&apos;N&apos; AND TO_NUMBER ( NVL (CALIFCLIENT.CALIFALIN,0)) &gt;= 3) 
          THEN
              (CASE WHEN (S.C1609-(S.C1610+S.C9661)) &gt; 0  
                  THEN (S.C1609-(S.C1610+S.C9661))
                ELSE
                  0
              END
                )
        ELSE
          0
      END
    ) AS MO_INTE_DEVE_SUST
FROM   
    STG.T_SALDOS S
    LEFT JOIN STG.T_CALIF_CLIENTES CALIFCLIENT
      ON CALIFCLIENT.C0902=S.C1803
      AND CALIFCLIENT.CALIFALIN&lt;&gt;&apos; &apos;
WHERE   S.C1604&lt;0 AND
    S.C9314=5
GROUP BY S.CUENTA</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
