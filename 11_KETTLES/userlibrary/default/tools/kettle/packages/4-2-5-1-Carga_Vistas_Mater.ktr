    <connection>OdsStgDS</connection>
    <sql>SELECT
 	SALDOS_SOL_CREDS.JTS_OID AS SALDOS_JTS_OID,
	ROUND((SELECT
      			CASE WHEN PROD.C6253=&apos;E&apos; THEN SALDOS_SOL_CREDS.C1632
