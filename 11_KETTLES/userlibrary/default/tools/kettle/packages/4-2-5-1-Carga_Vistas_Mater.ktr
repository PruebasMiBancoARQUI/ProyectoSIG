      			END
     		FROM STG.T_CO_PRODUCTOS PROD
     		WHERE PROD.C6250=SALDOS_SOL_CREDS.PRODUCTO),7) AS VL_TEA
FROM STG.T_SALDOS_SL_SOLICITUDCREDITO SALDOS_SOL_CREDS</sql>
