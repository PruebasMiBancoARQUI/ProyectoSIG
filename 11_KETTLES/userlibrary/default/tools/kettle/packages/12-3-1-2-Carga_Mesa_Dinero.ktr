    <sql>WITH TC_DATA AS
 (SELECT T2.SUCURSAL CO_SUCURSAL,
         T2.USUARIO,
         T2.ASIENTO ASIENTO,
         TO_CHAR(T2.FECHA_PROCESO, &apos;YYYYMM&apos;) PERIODO,
         T2.FECHA_PROCESO FECHA_PROCESO,
         T2.HORAFIN HORA_PROCESO,
         T2.CO_CLIENTE CO_CLIENTE,
         DECODE(T2.TIPO_OPERACION, &apos;V&apos;, &apos;VENTA&apos;, &apos;C&apos;, &apos;COMPRA&apos;, &apos;&apos;) TIPO_OPERACION_REAL,
         T2.MONTO_ORIGEN AS MONTO_ORIGEN,
         CO_MONE_ORIG AS CO_MONE_ORIG,
         T2.TI_OPERACION,
         T2.CO_OPERATIVA,
         T2.IN_CLIENTE,
         T2.MONTO_DESTINO MONTO_DESTINO,
         T2.CO_MONE_DEST AS CO_MONE_DEST,
         T2.TIPO_CAMBIO1 TIPO_CAMBIO,
         (SELECT BT.TC_COMPRA_LIM_CONCER
            FROM STG.T_CO_BITACORA_TIPOCAMBIO_01 BT
           WHERE BT.FECHA = T2.FECHA
             AND BT.HORA = T2.HORA) TC_COMPRA_LIM_CONCER,
         (SELECT BT.TC_VENTA_LIM_CONCER
            FROM STG.T_CO_BITACORA_TIPOCAMBIO_01 BT
           WHERE BT.FECHA = T2.FECHA
             AND BT.HORA = T2.HORA) TC_VENTA_LIM_CONCER,
         T2.FECHA FECHA,
         CASE
           WHEN T2.HORA IS NULL THEN
            (SELECT MAX(TC.HORA)
               FROM STG.T_CO_BITACORA_TIPOCAMBIO_01 TC
              WHERE TC.FECHA = (SELECT MAX(BT.FECHA)
                                  FROM STG.T_CO_BITACORA_TIPOCAMBIO_01 BT
                                 WHERE BT.FECHA &lt; T2.FECHA_PROCESO))
           ELSE
            T2.HORA
         END HORA
    FROM (SELECT T1.SUCURSAL,
                 T1.USUARIO,
                 T1.ASIENTO,
                 T1.FECHA_PROCESO,
                 T1.HORAFIN,
                 T1.COD_CLIENTE CO_CLIENTE,
                 T1.TIPO_OPERACION,
                 T1.MONTO_ORIGEN,
                 T1.CO_MONE_ORIG,
                 T1.TI_OPERACION,
                 T1.OPERACION AS CO_OPERATIVA,
                 T1.MONTO_DESTINO,
                 T1.CO_MONE_DEST,
                 T1.CLASE_TC,
                 T1.TIPO_CAMBIO1,
                 T1.OPERACION,
                 T1.IN_CLIENTE,
                 CASE T1.FECHA
                   WHEN T1.FECHA_PROCESO THEN
                    T1.FECHA
                   ELSE
                    (SELECT MAX(TC.FECHA)
                       FROM STG.T_CO_BITACORA_TIPOCAMBIO_01 TC
                      WHERE TC.FECHA &lt; T1.FECHA_PROCESO)
                 END FECHA,
                 CASE
                   WHEN T1.HORA IS NULL THEN
                    (SELECT MAX(TC.HORA)
                       FROM STG.T_CO_BITACORA_TIPOCAMBIO_01 TC
                      WHERE TC.FECHA =
                            (SELECT MAX(BT.FECHA)
                               FROM STG.T_CO_BITACORA_TIPOCAMBIO_01 BT
                              WHERE BT.FECHA &lt; T1.FECHA_PROCESO))
                   ELSE
                    T1.HORA
                 END HORA
            FROM (SELECT MV.SUCURSAL,
                         CASE
                           WHEN MV.TIPO_OPERACION LIKE &apos;%C%&apos; THEN
                            &apos;1&apos;
                           WHEN MV.TIPO_OPERACION LIKE &apos;%V%&apos; THEN
                            &apos;0&apos;
                           ELSE
                            &apos;0&apos;
                         END AS TI_OPERACION,
                         CASE
                           WHEN mv.CLASE_TC LIKE &apos;%NC%&apos; THEN
                            &apos;0&apos;
                           ELSE
                            &apos;1&apos;
                         END AS IN_CLIENTE,
                         MV.USUARIO,
                         MV.ASIENTO,
                         MV.FECHA_PROCESO,
                         A.HORAFIN,
                         MV.COD_CLIENTE,
                         MV.TIPO_OPERACION,
                         MV.MONTO_ORIGEN,
                         MV.MONEDA1 AS CO_MONE_ORIG,
                         MV.MONTO_DESTINO,
                         MV.MONEDA2 AS CO_MONE_DEST,
                         MV.CLASE_TC,
                         MV.TIPO_CAMBIO1,
                         MV.MONEDA1,
                         A.OPERACION,
                         (SELECT MAX(TC.FECHA)
                            FROM STG.T_CO_BITACORA_TIPOCAMBIO_01 TC
                           WHERE TC.FECHA &lt;= MV.FECHA_PROCESO
                             AND TC.HORA &lt;= TO_CHAR(A.HORAFIN, &apos;HH24:MI:SS&apos;)) FECHA,
                         (SELECT MAX(TC.HORA)
                            FROM STG.T_CO_BITACORA_TIPOCAMBIO_01 TC
                           WHERE TC.FECHA =
                                 (SELECT MAX(BT.FECHA)
                                    FROM STG.T_CO_BITACORA_TIPOCAMBIO_01 BT
                                   WHERE BT.FECHA = MV.FECHA_PROCESO)
                             AND TC.HORA &lt;= TO_CHAR(A.HORAFIN, &apos;HH24:MI:SS&apos;)) HORA
                    FROM STG.T_MOV_CAMBIOS_01 MV
                   INNER JOIN STG.T_ASIENTOS_07 A
                      ON A.ASIENTO = MV.ASIENTO
                     AND A.FECHAPROCESO = MV.FECHA_PROCESO
                     AND A.SUCURSAL = MV.SUCURSAL
                   WHERE MV.ESTADOPE = &apos;L&apos;
                     AND MV.FECHA_PROCESO = TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;, &apos;DD&#47;MM&#47;YYYY&apos;)
                     AND A.OPERACION IN (3504, 3618, 6202, 6203, 6236, 6237)) T1) T2),

REPORTE_TC as
 (SELECT SYSDATE                FE_ACTUALIZACION,
         A.CO_SUCURSAL          CO_SUCURSAL,
         A.FECHA_PROCESO,
         A.HORA_PROCESO,
         A.CO_CLIENTE,
         A.TI_OPERACION,
         A.IN_CLIENTE,
         A.MONTO_ORIGEN,
         A.CO_MONE_ORIG,
         A.MONTO_DESTINO,
         A.CO_MONE_DEST,
         A.TIPO_CAMBIO,
         A.TC_COMPRA_LIM_CONCER,
         A.TC_VENTA_LIM_CONCER,
         A.ASIENTO,
         A.USUARIO,
         CO_OPERATIVA,
         A.HORA
    FROM TC_DATA A),

TC_ANALISIS_1 AS
 (SELECT A.CO_SUCURSAL,
         A.FECHA_PROCESO,
         ASIENTO,
         USUARIO,
         A.HORA_PROCESO,
         A.CO_CLIENTE,
         A.TI_OPERACION,
         A.IN_CLIENTE,
         A.MONTO_ORIGEN,
         A.CO_MONE_ORIG,
         A.MONTO_DESTINO,
         CO_MONE_DEST,
         CO_OPERATIVA,
         A.TIPO_CAMBIO,
         A.TC_COMPRA_LIM_CONCER,
         A.TC_VENTA_LIM_CONCER,
         A.HORA
    FROM REPORTE_TC A),

TC_ANALISIS_2 AS
 (SELECT ASIENTO              AS NU_ASIENTO,
         FECHA_PROCESO        AS FE_PROCESO,
         CO_SUCURSAL,
         TI_OPERACION,
         IN_CLIENTE,
         CO_CLIENTE,
         MONTO_ORIGEN         AS MO_OPER_ORIG,
         CO_MONE_ORIG,
         MONTO_DESTINO        AS MO_OPER_DEST,
         CO_MONE_DEST,
         TIPO_CAMBIO          AS VL_TIPO_CAMB,
         USUARIO              AS CO_USUA_TOPA,
         HORA_PROCESO         AS TX_HORA_PROC,
         CO_OPERATIVA,
         TC_COMPRA_LIM_CONCER AS VL_TIPO_CAMB_CONC_COMP,
         TC_VENTA_LIM_CONCER  AS VL_TIPO_CAMB_CONC_VENT
    FROM TC_ANALISIS_1)

SELECT NU_ASIENTO,
       FE_PROCESO,
       CO_SUCURSAL,
       TI_OPERACION,
       IN_CLIENTE,
       CO_CLIENTE,
       MO_OPER_ORIG,
       CO_MONE_ORIG,
       MO_OPER_DEST,
       CO_MONE_DEST,
       VL_TIPO_CAMB,
       CO_USUA_TOPA,
       TX_HORA_PROC,
       CO_OPERATIVA,
       VL_TIPO_CAMB_CONC_COMP,
       VL_TIPO_CAMB_CONC_VENT,
       SYSDATE AS FE_ACTU_DIA
  FROM TC_ANALISIS_2
 ORDER BY CO_SUCURSAL, NU_ASIENTO</sql>
