  <step>
    <name>Leo T_CL_CLIENTES_01 Personas Naturales</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>WITH SUBQ1 AS(
              SELECT c2.C0902 AS CO_CLIENTE
              FROM STG.T_CL_CLIENTES_01 c2 
                                   JOIN STG.T_CL_CLIENTPERSONA cp2
                                        ON cp2.C1430=c2.C0902
                                   JOIN STG.T_CL_PERSONASFISICAS pf2
                                        ON cp2.C1431=pf2.C1401
                                           AND cp2.C1432=pf2.C1402
                                           AND cp2.C1435=pf2.C1403
                                   JOIN STG.T_CL_CONYUGES cy2
                                        ON  cy2.C1491 = pf2.C1401     
                                           AND cy2.C1492 = pf2.C1402
                                           AND cy2.C1502 = pf2.C1403
             WHERE c2.C1040=&apos;N&apos;
             GROUP BY c2.C0902
             HAVING COUNT(*)&gt;1                                                                                                                                                                                  
)
SELECT
	  c.C0902 AS CO_CLIENTE,
	  UPPER(c.C1000) AS DE_CLIENTE,
	  pf.C1401 AS TI_DOCUMENTO,
	  UPPER(o.DESCRIPCION) AS DE_TIPO_DOCU,
	  pf.C1402 AS NU_DOCUMENTO,
	  pf.C1403 AS CO_PAIS_DOCU,
	  cp.C1437 AS TI_PERSONA,
	  pf.C1408 AS TI_SEXO,
	  TRUNC(pf.C1420) AS FE_NACIMIENTO,
	  CASE WHEN c.C1022 = &apos;B&apos; THEN &apos;B&apos;
		   ELSE &apos;D&apos; 
	  END AS CO_BLOQUEO,
	  CASE WHEN c.C1022 = &apos;B&apos; THEN &apos;BLOQUEADO&apos;
		   ELSE &apos;DESBLOQUEADO&apos; 
	  END AS DE_BLOQUEO,
	  c.C1023 AS FE_APERTURA,
	  c.C1024 AS FE_CIERRE,
	  c.C1073 AS FE_BLOQUEO,
	  c.C1038 AS CO_ESTADO, 
	  UPPER(o3.DESCRIPCION) AS DE_ESTADO,
	  c.C1040 AS CO_TIPO_CLIE,
	  UPPER(o4.DESCRIPCION) AS DE_TIPO_CLIE,
	  c.C1048 AS CO_TIPO_CLIE_PASI,
	  UPPER(o5.DESCRIPCION) AS DE_TIPO_CLIE_PASI,
	  c.C1058 AS CO_ACTI_ECON_INTE,
	  c.C1840 AS CO_SECT_ECON_ALTE,
	  c.C3205 AS CO_ACTIVIDAD,
	  c.C1034 AS CO_ACTI_CIIU,
	  c.CANTPREST AS NU_CANT_PRES,
	  c.C8425 AS CO_SUCURSAL,
	  c.MED_REC_LUZ AS NU_MEDI_RECI_LUZ,
	  c.C1584 AS CO_SECT_ECON_INTE,
	  CASE c.EMPLEADO WHEN &apos;E&apos; THEN 1
			 ELSE 0
	  END AS IN_EMPLEADO,
	  c.TIPO_RESIDENCIA AS CO_TIPO_RESI,
	  UPPER(o7.DESCRIPCION) AS DE_TIPO_RESI,
	  pf.C8666 AS CO_SBS_CLIE,
