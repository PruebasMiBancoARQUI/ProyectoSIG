			 AND o9.NUMERODECAMPO=1411
		  LEFT JOIN STG.T_OPCIONES o10
			 ON o10.OPCIONINTERNA=pf.C1451
			 AND o10.NUMERODECAMPO=1451
		  JOIN SUBQ1
			 ON  SUBQ1.CO_CLIENTE=c.c0902     
WHERE
      c.C1040=&apos;N&apos;</sql>
