		  LEFT JOIN STG.T_OPCIONES o9
			 ON o9.OPCIONINTERNA=pf.C1411
			 AND o9.NUMERODECAMPO=1411
		  LEFT JOIN STG.T_OPCIONES o10
			 ON o10.OPCIONINTERNA=pf.C1451
			 AND o10.NUMERODECAMPO=1451
		  LEFT JOIN STG.T_CL_CONYUGES cy
			 ON  cy.C1491 = pf.C1401     
			 AND cy.C1492 = pf.C1402
			 AND cy.C1502 = pf.C1403
		  LEFT JOIN STG.T_CL_PERSONASFISICAS pf2
			 ON  cy.C1493 = pf2.C1401     
			 AND cy.C1494 = pf2.C1402
			 AND cy.C1503 = pf2.C1403			 
WHERE
      c.C1040=&apos;N&apos;
      AND NOT EXISTS (SELECT 1 FROM SUBQ1 WHERE SUBQ1.CO_CLIENTE=c.c0902)

UNION ALL
      
SELECT
