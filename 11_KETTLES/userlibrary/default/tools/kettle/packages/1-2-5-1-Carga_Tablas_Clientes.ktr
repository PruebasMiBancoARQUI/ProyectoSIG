  <step>
    <name>Leo T_CL_CLIENTES_01 Personas Juridicas</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>SELECT
	c.C0902 AS CO_CLIENTE,
	UPPER(c.C1000) AS DE_CLIENTE,
	pj.C1452 AS TI_DOCUMENTO,
	UPPER(o.DESCRIPCION) AS DE_TIPO_DOCU,
	pj.C1453 AS NU_DOCUMENTO,
	&apos;PE&apos; AS CO_PAIS_DOCU,
	cp.C1437 AS TI_PERSONA,
	&apos; &apos; AS TI_SEXO,
	TO_DATE(NULL) AS FE_NACIMIENTO,
	CASE WHEN c.C1022 = &apos;B&apos; THEN &apos;B&apos;
       	 ELSE &apos;D&apos; 
  	END AS CO_BLOQUEO,
  	CASE WHEN c.C1022 = &apos;B&apos; THEN &apos;BLOQUEADO&apos;
       	 ELSE &apos;DESBLOQUEADO&apos; 
  	END AS DE_BLOQUEO,
	c.C1023 AS FE_APERTURA,
	c.C1024 AS FE_CIERRE,
	c.C1073 AS FE_BLOQUEO,
	c.C1038 AS CO_ESTADO, 
	UPPER(o3.DESCRIPCION) AS DE_ESTADO,
	c.C1040 AS CO_TIPO_CLIE,
	UPPER(o4.DESCRIPCION) AS DE_TIPO_CLIE,
	c.C1048 AS CO_TIPO_CLIE_PASI,
	UPPER(o5.DESCRIPCION) AS DE_TIPO_CLIE_PASI,
	c.C1058 AS CO_ACTI_ECON_INTE,
	c.C1840 AS CO_SECT_ECON_ALTE,
	c.C3205 AS CO_ACTIVIDAD,
	c.C1034 AS CO_ACTI_CIIU,
	c.CANTPREST AS NU_CANT_PRES,
	c.C8425 AS CO_SUCURSAL,
	c.MED_REC_LUZ AS NU_MEDI_RECI_LUZ,
	c.C1584 AS CO_SECT_ECON_INTE,
	CASE c.EMPLEADO WHEN &apos;E&apos; THEN 1
				 ELSE 0
	END AS IN_EMPLEADO,
	c.TIPO_RESIDENCIA AS CO_TIPO_RESI,
	UPPER(o7.DESCRIPCION) AS DE_TIPO_RESI,
	pj.C8667 AS CO_SBS_CLIE,
	&apos;J&apos; AS CO_CATE_PERS,
	&apos;PERSONA JURIDICA&apos; AS DE_CATE_PERS,
	9 AS CO_MAGN_EMPR,
	&apos;NO DECLARADO&apos; AS DE_MAGN_EMPR,
	&apos;X&apos; AS CO_CLAS_INDE,
	&apos;NO DECLARADO&apos; AS DE_CLAS_INDE,
	117 AS CO_PROFESION,
	&apos;NO DECLARA&apos; AS DE_PROFESION,
	&apos;N&apos; AS CO_NIVE_EDUC,
	&apos;NINGUNO&apos; AS DE_NIVE_EDUC,
	&apos; &apos; AS CO_ESTA_CIVI,
	cp.ESTADO AS CO_BASE_NEGA,
	&apos; &apos; AS CO_CLAS_DEPE,
	TO_DATE(NULL)  AS FE_INGRESO,
	&apos; &apos; AS IN_MIGRADO,
	pj.C1513 AS CO_NATU_JURI,
	TO_DATE(NULL)  AS FE_APER_PRIM_CRED,
	&apos; &apos; AS TI_DOCU_CONY,
	&apos; &apos; AS NU_DOCU_CONY,
	&apos; &apos; AS CO_PAIS_CONY,
	&apos; &apos; AS DE_CONYUGE,
	&apos; &apos; AS IN_SEPA_BIEN,
	SYSDATE AS FE_ACTU_DIA
FROM STG.T_CL_CLIENTES_01 c
	 JOIN STG.T_CL_CLIENTPERSONA cp
	 	ON cp.C1430=c.C0902
	  JOIN STG.T_CL_PERSONASJURIDICAS pj
	 	ON cp.C1431=pj.C1452
	 	AND cp.C1432=pj.C1453
	 LEFT JOIN STG.T_OPCIONES o
	 	ON o.OPCIONINTERNA=pj.C1452 
	 	AND o.NUMERODECAMPO=1452 
	 LEFT JOIN STG.T_OPCIONES o3
	 	ON o3.OPCIONINTERNA=c.C1038
	 	AND o3.NUMERODECAMPO=1038
	 LEFT JOIN STG.T_OPCIONES o4
	 	ON o4.OPCIONINTERNA=c.C1040
	 	AND o4.NUMERODECAMPO=1040
	 LEFT JOIN STG.T_OPCIONES o5
	 	ON o5.OPCIONINTERNA=c.C1048
	 	AND o5.NUMERODECAMPO=1048
	 LEFT JOIN STG.T_OPCIONES o7
	 	ON o7.OPCIONINTERNA=c.TIPO_RESIDENCIA
	 	AND o7.NUMERODECAMPO=4207
WHERE
	c.C1040=&apos;J&apos;</sql>
    <limit>0</limit>
    <lookup/>
