	  CASE WHEN c.C1022 = &apos;B&apos; THEN &apos;BLOQUEADO&apos;
		   ELSE &apos;DESBLOQUEADO&apos; 
	  END AS DE_BLOQUEO,
	  c.C1023 AS FE_APERTURA,
	  c.C1024 AS FE_CIERRE,
	  c.C1073 AS FE_BLOQUEO,
	  c.C1038 AS CO_ESTADO, 
	  UPPER(o3.DESCRIPCION) AS DE_ESTADO,
	  c.C1040 AS CO_TIPO_CLIE,
	  UPPER(o4.DESCRIPCION) AS DE_TIPO_CLIE,
	  c.C1048 AS CO_TIPO_CLIE_PASI,
	  UPPER(o5.DESCRIPCION) AS DE_TIPO_CLIE_PASI,
	  c.C1058 AS CO_ACTI_ECON_INTE,
	  c.C1840 AS CO_SECT_ECON_ALTE,
	  c.C3205 AS CO_ACTIVIDAD,
	  c.C1034 AS CO_ACTI_CIIU,
	  c.CANTPREST AS NU_CANT_PRES,
	  c.C8425 AS CO_SUCURSAL,
	  c.MED_REC_LUZ AS NU_MEDI_RECI_LUZ,
	  c.C1584 AS CO_SECT_ECON_INTE,
	  CASE c.EMPLEADO WHEN &apos;E&apos; THEN 1
			 ELSE 0
	  END AS IN_EMPLEADO,
	  c.TIPO_RESIDENCIA AS CO_TIPO_RESI,
	  UPPER(o7.DESCRIPCION) AS DE_TIPO_RESI,
	  pf.C8666 AS CO_SBS_CLIE,
	  NVL(TRIM(pf.C1410),&apos;N&apos;) AS CO_CATE_PERS,
	  NVL(UPPER(o8.DESCRIPCION), &apos;NINGUNA&apos;) AS DE_CATE_PERS,
	  NVL(pf.C1411,9) AS CO_MAGN_EMPR,
	  NVL(UPPER(o9.DESCRIPCION),&apos;NO DECLARADO&apos;) AS DE_MAGN_EMPR,
	  CASE WHEN pf.C1451=&apos; &apos; THEN &apos;X&apos;
		   ELSE NVL(pf.C1451,&apos;X&apos;) 
	  END AS CO_CLAS_INDE,
	  NVL(UPPER(o10.DESCRIPCION),&apos;NO DECLARADO&apos;) AS DE_CLAS_INDE,
	  NVL(pf.C1499,117) AS CO_PROFESION,
	  NVL(UPPER(p.C1222),&apos;NO DECLARA&apos;) AS DE_PROFESION,
	  CASE WHEN pf.C1448=&apos; &apos; THEN &apos;N&apos;
		 ELSE NVL(pf.C1448,&apos;N&apos;) 
	  END AS CO_NIVE_EDUC,
	  NVL(UPPER(ne.DESCRIPCION),&apos;NINGUNO&apos;) AS DE_NIVE_EDUC,
	  pf.C1409 AS CO_ESTA_CIVI,
	  cp.ESTADO AS CO_BASE_NEGA,
	  pf.C1449 AS CO_CLAS_DEPE,
	  pf.C1599 AS FE_INGRESO,
	  pf.MIGRADO AS IN_MIGRADO,
	  &apos; &apos; AS CO_NATU_JURI,
	  NULL AS FE_APER_PRIM_CRED,
	  NULL AS TI_DOCU_CONY,
	  NULL AS NU_DOCU_CONY,
	  NULL AS CO_PAIS_CONY,
	  &apos; &apos; AS DE_CONYUGE,
	  NULL AS IN_SEPA_BIEN,
	  SYSDATE AS FE_ACTU_DIA 
FROM STG.T_CL_CLIENTES_01 c
		   JOIN STG.T_CL_CLIENTPERSONA cp
			 ON cp.C1430=c.C0902
		   JOIN STG.T_CL_PERSONASFISICAS pf
			 ON cp.C1431=pf.C1401
			 AND cp.C1432=pf.C1402
			 AND cp.C1435=pf.C1403
		   LEFT JOIN STG.T_OPCIONES o
			 ON o.OPCIONINTERNA=pf.C1401
			 AND o.NUMERODECAMPO=1401
		   LEFT JOIN STG.T_OPCIONES o3
			 ON o3.OPCIONINTERNA=c.C1038
			 AND o3.NUMERODECAMPO=1038
		   LEFT JOIN STG.T_OPCIONES o4
			 ON o4.OPCIONINTERNA=c.C1040
			 AND o4.NUMERODECAMPO=1040
		   LEFT JOIN STG.T_OPCIONES o5
			 ON o5.OPCIONINTERNA=c.C1048
			 AND o5.NUMERODECAMPO=1048
		   LEFT JOIN STG.T_OPCIONES o7
			 ON o7.OPCIONINTERNA=c.TIPO_RESIDENCIA
			 AND o7.NUMERODECAMPO=4207
		   LEFT JOIN STG.T_TC_PROFECIONES p
			 ON p.C1221=pf.C1499
		   LEFT JOIN STG.T_TC_NIVELES_EDUCATIVOS ne
			 ON ne.COD_NIVEL=pf.C1448
		   LEFT JOIN STG.T_OPCIONES o8
			 ON o8.OPCIONINTERNA=pf.C1410
