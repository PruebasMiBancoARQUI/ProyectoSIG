  <step>
    <name>Leo HM_EGP</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>WITH 
     SUBQ1 AS
     (
      SELECT CO_CLIENTE, 
             SUM(MO_TOTA_DEUD) AS MO_TOTA_DEUD
      FROM ODS.HM_EGP
      WHERE NU_PERI_MES = (
                           SELECT TO_NUMBER(TO_CHAR(TO_DATE(TO_CHAR(FECHAPROCESO,&apos;YYYYMM&apos;)||&apos;01&apos;,&apos;YYYYMMDD&apos;)-1,&apos;YYYYMM&apos;))
                           FROM STG.T_PARAMETROS
                           WHERE FECHAPROCESO = TO_DATE(&apos;${FECHAPROCESOEDY_S_P}&apos;,&apos;YYYYMMDD&apos;)
                                 AND TO_NUMBER(TO_CHAR(FECHAPROXIMOPROCESO,&apos;YYYYMM&apos;)) &gt; TO_NUMBER(TO_CHAR(FECHAPROCESO,&apos;YYYYMM&apos;))
                          )
      GROUP BY CO_CLIENTE
     )
SELECT TO_NUMBER(&apos;${FECHAPROCESOEDY_S_P_M}&apos;) AS NU_PERI_MES,
       TO_NUMBER(TO_CHAR(a.FE_SALDO,&apos;YYYYMM&apos;)) AS NU_PERI_CART,
       a.NU_PRESTAMO,
       a.CO_CLIENTE,
       a.CO_ESTA_OPER AS CO_ESTA_PRES,
       a.CO_PRODUCTO,
       a.CO_REFINANCIADO AS CO_REFINANCIACION,
       a.CO_MONEDA,
       a.CO_SUCURSAL AS CO_AGENCIA,
       d.CO_TIPO_CRED AS CO_TIPO_PRES,
       a.CO_CRITERIO,
       a.CO_ANALISTA AS CO_FUNC_RESP,
       CASE
          WHEN a.CO_ESTA_OPER =&apos;C&apos;   THEN 1
          WHEN a.NU_DIAS_ATRA &lt;= 0   THEN 2
          WHEN a.NU_DIAS_ATRA &lt;= 30  THEN 3
          WHEN a.NU_DIAS_ATRA &lt;= 60  THEN 4
          WHEN a.NU_DIAS_ATRA &lt;= 90  THEN 5
          WHEN a.NU_DIAS_ATRA &lt;= 120 THEN 6
          WHEN a.NU_DIAS_ATRA &lt;= 150 THEN 7
          WHEN a.NU_DIAS_ATRA &gt;= 151 THEN 8
          ELSE 9
       END AS CO_TRAMO,
       TRUNC(MONTHS_BETWEEN(TO_DATE(TO_CHAR(a.FE_SALDO,&apos;YYYYMM&apos;)||&apos;01&apos;,&apos;YYYYMMDD&apos;), TO_DATE(TO_CHAR(a.FE_DESEMBOLSO,&apos;YYYYMM&apos;),&apos;YYYYMM&apos;))) AS NU_COSECHA,
       a.NU_CUOT_PACT AS NU_CUOT_TOTA,
       a.NU_CUOT_PAGA AS NU_CUOT_PAGA,
       a.NU_LINE_CRED,
       TRUNC(MONTHS_BETWEEN(TO_DATE(&apos;${FECHAPROCESOEDY_S_P_M}01&apos;,&apos;YYYYMMDD&apos;), TO_DATE(TO_CHAR(c.FE_NACIMIENTO,&apos;YYYYMM&apos;),&apos;YYYYMM&apos;))&#47;12) AS NU_EDAD, 
       a.MO_DESEMBOLSO,
       a.MO_SALD_ACTU,
       e.MO_TOTA_DEUD,                        -- El campo se carga con el JOIN
       a.MO_SALD_DISP_LINE,
       CASE
          WHEN TO_CHAR(a.FE_CASTIGO,&apos;YYYYMM&apos;) = TO_CHAR(a.FE_SALDO,&apos;YYYYMM&apos;) THEN a.MO_SALD_ACTU
          ELSE 0
       END AS MO_SALD_CAST_MES,
       CASE
          WHEN b.CO_ESTA_OPER = &apos;C&apos; OR ((CASE WHEN b.NU_DIAS_ATRA &lt;= 0 THEN -1 ELSE FLOOR((b.NU_DIAS_ATRA-1)&#47;30) END) - 
                                        (CASE WHEN a.NU_DIAS_ATRA &lt;= 0 THEN -1 ELSE FLOOR((a.NU_DIAS_ATRA-1)&#47;30) END)) &gt; 0 THEN
               CASE
                  WHEN b.MO_SALD_ACTU &gt; a.MO_SALD_ACTU THEN a.MO_SALD_ACTU
                  ELSE b.MO_SALD_ACTU
               END
          ELSE  0
       END AS MO_SALD_PASE,
       a.MO_INTE_DEVE_VIGE AS MO_INTE_DEVE_VIGE,
       a.MO_INTE_DEVE_SUST AS MO_INTE_DEVE_SUST,
       CASE
          WHEN a.CO_ESTA_OPER = &apos;C&apos;  THEN 1
          ELSE 0
       END AS IN_CASTIGO,
       CASE
          WHEN a.CO_ESTA_OPER &lt;&gt; &apos;C&apos; THEN 1
          ELSE 0
       END AS IN_ACTIVO,
       (CASE WHEN b.NU_DIAS_ATRA &lt;= 0 THEN -1 ELSE FLOOR((b.NU_DIAS_ATRA-1)&#47;30) END) -
       (CASE WHEN a.NU_DIAS_ATRA &lt;= 0 THEN -1 ELSE FLOOR((a.NU_DIAS_ATRA-1)&#47;30) END) AS NU_TRAM_PASE,
       a.FE_DESEMBOLSO,
       a.FE_VENCIMIENTO,
       a.FE_ULTI_PAGO,
       a.FE_CASTIGO,
       a.FE_VENC_LINE,
       (((POWER(1+((a.VL_TEA&#47;100)),1&#47;12))-1)*12*100) AS VL_TNA,
       a.MO_INTE_COMP_ATRA,
       a.MO_INTE_MORA_PEND,
       SYSDATE AS FE_ACTU_MES
FROM  ODS.HM_EGP a
      LEFT JOIN ODS.HM_EGP PARTITION (P_HM_EGP_${FECHAPROCESOEDY_S_P_M}) b ON a.NU_PRESTAMO = b.NU_PRESTAMO
      LEFT JOIN ODS.MD_CLIENTES c ON c.CO_CLIENTE  = a.CO_CLIENTE
      LEFT JOIN ODS.MD_CRITERIO d ON d.CO_CRITERIO = a.CO_CRITERIO
      LEFT JOIN ODS.SUBQ1       e ON e.CO_CLIENTE  = a.CO_CLIENTE
WHERE a.NU_PERI_MES = (
                       SELECT TO_NUMBER(TO_CHAR(TO_DATE(TO_CHAR(FECHAPROCESO,&apos;YYYYMM&apos;)||&apos;01&apos;,&apos;YYYYMMDD&apos;)-1,&apos;YYYYMM&apos;))
                       FROM STG.T_PARAMETROS
                       WHERE FECHAPROCESO = TO_DATE(&apos;${FECHAPROCESOEDY_S_P}&apos;,&apos;YYYYMMDD&apos;)
                             AND TO_NUMBER(TO_CHAR(FECHAPROXIMOPROCESO,&apos;YYYYMM&apos;)) &gt; TO_NUMBER(TO_CHAR(FECHAPROCESO,&apos;YYYYMM&apos;))
                      )</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
