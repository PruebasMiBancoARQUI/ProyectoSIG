    <sql>------------------------------------------------------------------------------------------------------------
-- Modificaciones
-- Responsable : Adrian Huamani Bonilla
-- Fecha       : 08&#47;07&#47;2020
-- Descripción : Se agregan 2 campos que serán utilizados en el Datamart de Riesgos
------------------------------------------------------------------------------------------------------------
--@001 AHB CASO-21672: Se agrega los campos TMP3.INTE_COMP y TMP3.MORA_CONT

WITH 
TMP_AHORRO as
--(SELECT S.C1803 AS CO_CLIENTE, S.JTS_OID AS IN_SALD_JTS_OID
--FROM STG.T_TMP_SALDOS_04 S
--JOIN STG.T_TMP_SALDOS_DIARIOS_01 SD ON S.JTS_OID = SD.SALDO_JTS_OID
--LEFT JOIN STG.T_TMP_HISTORIA_VISTA_EGD HV  ON HV.SALDO_JTS_OID = S.JTS_OID AND HV.FECHA_PROCESADO = TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;, &apos;DD&#47;MM&#47;YYYY&apos;)
--LEFT JOIN  STG.T_CO_PRODUCTOS PROD ON PROD.C6250=S.PRODUCTO AND PROD.C6252 = S.C9314   
--WHERE  S.C9314 IN (2,3) AND(SD.SALDO_AL_CORTE &lt;&gt; 0  OR (SD.SALDO_AL_CORTE = 0 AND SD.INTERESESPAGO &lt;&gt; 0))
--AND S.COD_CAN !=&apos;1&apos; AND PROD.C6252=3) 

(SELECT EGD.CO_CLIENTE AS CO_CLIENTE  FROM ODS.UD_EGD EGD WHERE IN_ESTA_PASI !=1
             AND  EGD.CO_TIPO_PASI=3  GROUP BY EGD.CO_CLIENTE )
SELECT DISTINCT
  TMP1.NU_PERI_MES,
  TMP1.FE_SALDO,         
  TMP1.FE_PROCESO,       
  TMP1.NU_PRESTAMO,      
  TMP1.CO_CLIENTE,       
  TMP1.IN_SIN_HIPO_INSC, 
  TMP1.NU_PLAZ_DIAS,
  TMP1.VL_TEA,
  TMP1.FE_VENCIMIENTO,   
  TMP1.FE_DESEMBOLSO,  
  TMP1.FE_ULTI_PAGO,          
  TMP1.FE_CASTIGO,       
  TMP1.NU_DIAS_ATRA,     
  TMP1.MO_DESEMBOLSO,    
  TMP1.MO_SALD_VENC,     
  TMP1.MO_SALD_VIGE,     
  TMP1.MO_SALD_ACTU,
  NVL((SELECT IDV.MO_INTE_DEVE_VIGE FROM STG.T_INTE_DEVE_VIGE IDV WHERE IDV.SALDOS_JTS_OID = TMP1.SALDOS_JTS_OID),0) AS MO_INTE_DEVE_VIGE,
  NVL((SELECT IDS.MO_INTE_DEVE_SUST FROM STG.T_INTE_DEVE_SUST IDS WHERE IDS.SALDOS_JTS_OID = TMP1.SALDOS_JTS_OID),0) AS MO_INTE_DEVE_SUST,
  TMP1.NU_CUOT_PAGA,     
  TMP1.MO_SALD_CAPI_MN,  
  TMP1.NU_LINE_CRED,     
  TMP1.MO_SALD_DISP_LINE,
  TMP1.FE_VENC_LINE,     
  TMP2.TI_PRESTAMO,       
  TMP2.CO_MONEDA,         
  TMP2.CO_ESTA_OPER,
  TMP2.CO_SUCURSAL,       
  TMP2.CO_ANALISTA,
  TMP2.CO_USUA_TOPA,
  TMP2.CO_CARG_ANAL,
  TMP2.CO_PUES_ANAL,
  TMP2.CO_SUCU_ANAL,       
  TMP2.CO_PRODUCTO,       
  TMP2.CO_CRITERIO,       
  TMP2.NU_SOLICITUD,      
  TMP2.NU_GRUP_SOLI,      
  TMP2.CO_SUCU_PADR,      
  TMP2.CO_ANAL_ORIG,
  TMP2.CO_CARG_ANAL_ORIG,
  TMP2.CO_PUES_ANAL_ORIG,
  TMP2.CO_SUCU_ANAL_ORIG,     
  TMP2.TI_CLIENTE,        
  nvl(TMP2.NU_CANT_EMPR,0) AS NU_CANT_EMPR,      
  nvl(TMP2.MO_SALD_SIST_FINA,0) AS MO_SALD_SIST_FINA, 
  nvl(TMP2.NU_PORC_EXCL,0) AS NU_PORC_EXCL ,      
  TMP2.NU_PERI_GRAC,      
  TMP2.CO_TIPO_GRAC,      
  TMP2.TI_EXCEPCION,
  TMP3.VL_TCEA,          
  ROUND((SELECT 
        CASE 
          WHEN PROD.C6253=&apos;E&apos; THEN TMP3.VL_TASA_MINI
               WHEN PROD.C6253=&apos;M&apos; THEN (((POWER((1+((TMP3.VL_TASA_MINI&#47;12)&#47;100)),12))-1)*100)
               WHEN PROD.C6253=&apos;N&apos; THEN TMP3.VL_TASA_MINI
            END
          FROM STG.T_CO_PRODUCTOS PROD
          WHERE PROD.C6250=TMP2.CO_PRODUCTO
  ),7) AS VL_TASA_MINI, 
  ROUND((SELECT 
        CASE 
          WHEN PROD.C6253=&apos;E&apos; THEN TMP3.VL_TASA_MAXI
               WHEN PROD.C6253=&apos;M&apos; THEN (((POWER((1+((TMP3.VL_TASA_MAXI&#47;12)&#47;100)),12))-1)*100)
               WHEN PROD.C6253=&apos;N&apos; THEN TMP3.VL_TASA_MAXI
            END
          FROM STG.T_CO_PRODUCTOS PROD
          WHERE PROD.C6250=TMP2.CO_PRODUCTO
  ),7) AS VL_TASA_MAXI,      
  TMP3.NU_DIAS_TOTA_ATRA,
  TMP3.NU_MAXI_DIAS_ATRA, 
  TMP3.NU_CUOT_PACT, 
  TMP3.CO_USUA_APRO_SOLI,
  TMP3.IN_FOGAPI, 
  TMP3.ST_FOGAPI,        
  TMP3.IN_SALD_JTS_OID,
  TMP3.IN_GARA_ACTI,      
  TMP3.IN_PAGO_PARC,      
  TMP3.MO_DEUD_VENC,      
  TMP3.MO_TOTA_DEUD,      
  TMP3.NU_CUOT_VENC,      
  TMP3.FE_VENC_CUOT,      
  TMP3.NU_DIAS_MORA_MES,
  TMP3.MO_PAGO_TOTA_MES,  
  TMP3.MO_PAGO_CAPI_MES,  
  TMP3.MO_PAGO_COND_CAPI, 
  TMP3.MO_PAGO_COND_TOTA, 
  TMP3.IN_CONDONACION,    
  TMP3.MO_SALD_CUEN_INTE,
  TMP3.IN_GARA_REAL,      
  TMP3.MO_SALD_RESU,      
  TMP3.VL_TASA_ICM,       
  TMP3.VL_TASA_INTE_MOR,
  TMP3.MO_TOTA_GAST,      
  TMP3.VL_TIPO_CAMB_OFIC,
  TMP4.CO_ACTI_ECON_INTE,
  TMP4.CO_SECT_ECON_ALTE,
  TMP4.CO_ACTIVIDAD,
  TMP4.CO_ACTI_CIIU,
  TMP4.CO_SECT_ECON_INTE,
  TMP4.IN_EMPLEADO,
  TMP4.NU_EMPR_REPO,
  TMP4.MO_TOTA_SIST_FINA,
  TMP4.FE_PROC_ULTI_PAGO,
  (SELECT FECHAVALOR FROM STG.T_BS_HISTORIA_PLAZO_SUMMARY3 BSHPSUMMARY3 WHERE BSHPSUMMARY3.SALDOS_JTS_OID=TMP1.SALDOS_JTS_OID) AS FE_PSJE_JUDI,
    TMP1.FE_PSJE_VENC,
    TMP1.IN_MIGRADO,
    TMP1.IN_REFINANCIADO,
    TMP2.IN_SINIESTRO,
    TMP1.MO_CUOTA,
    TMP1.VL_DESGRAVAMEN,
    TMP1.VL_TASA_OPTI,
    TMP1.CO_PROD_ORIG,
 CASE WHEN TMP1.CO_CLIENTE=AH.CO_CLIENTE THEN 1 ELSE 0 END AS IN_AHORRO,
    TMP2.CO_DESTINO AS CO_DESTINO,
      TMP1.CO_REFINANCIADO,
  SYSDATE AS FE_ACTU_DIA,
--@001 Ini: Se agrega los campos TMP3.INTE_COMP y TMP3.MORA_CONT
  TMP3.INTE_COMP AS MO_INTE_COMP_ATRA,
  TMP3.MORA_CONT AS MO_INTE_MORA_PEND     
--@001 Fin
FROM 
  STG.TMP_EGP_1 TMP1
  JOIN STG.TMP_EGP_2 TMP2  ON TMP1.SALDOS_JTS_OID=TMP2.SALDOS_JTS_OID
  JOIN STG.TMP_EGP_3 TMP3  ON TMP1.SALDOS_JTS_OID=TMP3.SALDOS_JTS_OID
  JOIN STG.TMP_EGP_4 TMP4  ON TMP1.SALDOS_JTS_OID=TMP4.SALDOS_JTS_OID
  LEFT JOIN TMP_AHORRO AH ON AH.CO_CLIENTE=TMP1.CO_CLIENTE</sql>
