    <connection>OdsStgDS</connection>
    <sql>--Caso Modelo de Admisión-Riesgos
--@001 CHC CASO-21670 Incidencias Despliegue de Admisión. 
WITH DESEMBOLSO_REF AS
 (SELECT NU_SOLICITUD,
         NU_PRESTAMO,
         CO_CLIENTE,
         FE_DESEMBOLSO,
         CO_REFINANCIACION
    FROM STG.T_DESEMBOLSO
   WHERE FE_DESEMBOLSO = TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;, &apos;DD&#47;MM&#47;YYYY&apos;)
     AND CO_REFINANCIACION IN (&apos;R&apos;, &apos;P&apos;))
SELECT r0.CRE_NUMSOLICITUD AS NU_SOLICITUD,
       fin.NU_PRESTAMO AS NU_PRESTAMO,
       fin.CO_CLIENTE AS CO_CLIENTE,
       fin.FE_DESEMBOLSO AS FE_DESEMBOLSO,
--@001       fin.CO_REFINANCIACION AS CO_TIPO_REFI,
       fin.CO_REFINANCIACION AS CO_REFINANCIADO, 
--@001 FIN           
       ini.NU_PRESTAMO AS NU_PRES_CANC,
       ini.NU_SOLICITUD AS NU_SOLI_CANC,
       r0.CRE_MONEDA AS CO_MONE_CANC,
       r0.CRE_PRODUCTO AS CO_PROD_CANC,
       ini.CO_CLIENTE AS CO_CLIE_CANC,
       r0.CRE_MONTOCANC AS MO_CRED_CANC,
--@001       NVL((r0.CRE_MONTOCANC - r1.CRE_MONTOCANC), r0.CRE_MONTOCANC) AS MO_CRED_IND_CANC,
       NVL((r0.CRE_MONTOCANC - r1.CRE_MONTOCANC), r0.CRE_MONTOCANC) AS MO_CRED_INDI_CANC,
--@001 FIN
       r0.CRE_MOTIVOCANC AS CO_MOTI_CANC,
--@001        
--      (SELECT DESCRIPCION
--          FROM STG.T_OPCIONES
--         WHERE NUMERODECAMPO = 1977
--           AND OPCIONINTERNA = r0.CRE_MOTIVOCANC) AS DE_MOTI_CANC,
--@001 FIN           
       r0.ORDEN AS NU_ORDE_PRES_CANC,
       NVL((r0.CRE_MONTOCANC - r1.CRE_MONTOCANC), r0.CRE_MONTOCANC) &#47; MO_CRED_TOT_CANC AS VL_PORC_PRES_CANC,
       SYSDATE AS FE_ACTU_DIA,
       TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;, &apos;DD&#47;MM&#47;YYYY&apos;) AS FE_PROCESO
  FROM DESEMBOLSO_REF fin
 INNER JOIN (SELECT a.CRE_NUMSOLICITUD,
                    a.CRE_MONEDA,
                    a.CRE_PRODUCTO,
                    a.CRE_MONTOCANC,
                    CRE_CUENTA,
                    CRE_MOTIVOCANC,
                    (ROW_NUMBER() OVER(PARTITION BY a.CRE_NUMSOLICITUD ORDER BY
                                       a.CRE_MONTOCANC)) ORDEN,
                    LAST_VALUE(a.CRE_MONTOCANC) OVER(PARTITION BY a.CRE_NUMSOLICITUD ORDER BY a.CRE_MONTOCANC RANGE BETWEEN UNBOUNDED PRECEDING AND UNBOUNDED FOLLOWING) MO_CRED_TOT_CANC
               FROM STG.T_SL_SOLICITUDCREDCANCELA a
              INNER JOIN DESEMBOLSO_REF f
                 ON a.CRE_NUMSOLICITUD = f.NU_SOLICITUD) r0
    ON r0.CRE_NUMSOLICITUD = fin.NU_SOLICITUD
  LEFT JOIN (SELECT CRE_NUMSOLICITUD,
                    CRE_MONTOCANC,
                    (ROW_NUMBER() OVER(PARTITION BY CRE_NUMSOLICITUD ORDER BY
                                       CRE_MONTOCANC)) + 1 ORDEN_SIG
               FROM STG.T_SL_SOLICITUDCREDCANCELA
              INNER JOIN DESEMBOLSO_REF f
                 ON CRE_NUMSOLICITUD = f.NU_SOLICITUD) r1
    ON r0.CRE_NUMSOLICITUD = r1.CRE_NUMSOLICITUD
   AND r0.ORDEN = r1.ORDEN_SIG
  INNER JOIN ODS.HD_DESEMBOLSO ini
    ON r0.CRE_CUENTA = ini.NU_PRESTAMO
</sql>
