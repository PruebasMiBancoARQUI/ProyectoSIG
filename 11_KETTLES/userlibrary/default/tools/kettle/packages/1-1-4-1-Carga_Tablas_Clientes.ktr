  <step>
    <name>Leo CL_CLIENTPERSONA</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>TopazRepDS</connection>
    <sql>SELECT
  cp.C1430 AS C1430,
  cp.C1437 AS C1437,
  cp.C1431 AS C1431,
  cp.C1432 AS C1432,
  cp.C1435 AS C1435,
  nvl(rc.ESTADO,0) AS ESTADO
FROM EDYFICAR.CL_CLIENTPERSONA cp
  LEFT JOIN (SELECT  
                  a.TIPODOC AS TIPODOC,
                  a.NRODOCUMENTO AS NRODOCUMENTO,
                  a.CODPAIS AS CODPAIS,
                  max(a.ESTADO) AS ESTADO     
             FROM EDYFICAR.RC_BASENEGMOTIVO a
             WHERE a.FECHAINGRESO = (SELECT max(b.FECHAINGRESO) AS  FECHAINGRESO 
                                   FROM EDYFICAR.RC_BASENEGMOTIVO b
                                   WHERE b.TIPODOC = a.TIPODOC
                                   AND b.NRODOCUMENTO = a.NRODOCUMENTO
                                   AND b.CODPAIS = a.CODPAIS
                                   AND b.TZ_LOCK = 0)
              AND NOT EXISTS (
                                         SELECT 1
                                         FROM EDYFICAR.RC_BASENEGMOTIVO b2
                                         WHERE b2.TIPODOC=a.TIPODOC
                                               AND b2.NRODOCUMENTO = a.NRODOCUMENTO
                                               AND b2.CODPAIS=a.CODPAIS
                                               AND b2.TZ_LOCK=0
                                               AND b2.FECHAINGRESO=a.FECHAINGRESO
                                               AND b2.HORAINGRESO&gt;a.HORAINGRESO
                                    )

               AND a.TZ_LOCK = 0
