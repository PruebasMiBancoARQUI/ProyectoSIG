    <connection>OdsStgDS</connection>
    <sql>WITH
  SUBQ1 AS
     (SELECT CO_CLIENTE
       FROM ODS.HM_EGP PARTITION (P_HM_EGP_${FECHAPROCESOEDY_S_P_M})
      GROUP BY CO_CLIENTE
      )

SELECT TO_NUMBER(&apos;${FECHAPROCESOEDY_S_P_M}&apos;) AS NU_PERI_MES,
       eg.CO_CLIENTE        AS CO_CLIENTE,
       cl.DE_CLIENTE        AS NO_CLIENTE,
       cl.TI_DOCUMENTO      AS TI_DOCUMENTO,
       cl.NU_DOCUMENTO      AS NU_DOCUMENTO,
       cl.TI_SEXO           AS TI_SEXO,
       cl.FE_NACIMIENTO     AS FE_NACIMIENTO,
       cl.CO_SBS_CLIE       AS CO_SBS,
       cl.CO_PAIS_DOCU      AS CO_PAIS_DOCU,
       cl.CO_CATE_PERS      AS CO_CATE_PERS,
       cl.DE_CATE_PERS      AS DE_CATE_PERS,
       cl.CO_CLAS_DEPE      AS CO_CLAS_DEPE,
       cd.DE_CLAS_DEPE      AS DE_CLAS_DEPE,
       cl.CO_ESTA_CIVI      AS CO_ESTA_CIVI,
       ec.DE_ESTA_CIVI      AS DE_ESTA_CIVI,
       sc.CO_SEGM_COBR      AS CO_SEGM_COBR,
       sc.DE_SEGM_COBR      AS DE_SEGM_COBR,
       sc.CO_SEGM_MORA      AS CO_SEGM_MORA,
       sc.DE_SEGM_MORA      AS DE_SEGM_MORA,
       cc.CO_CALI_INTE_FINA AS CO_CALI_INTE_FINA,
       cc.DE_CALI_INTE_FINA AS DE_CALI_INTE_FINA,
       cc.CO_CALI_SIST_FINA AS CO_CALI_SIST_FINA,
       cc.DE_CALI_SIST_FINA AS DE_CALI_SIST_FINA,
       cc.CO_CALI_ALIN      AS CO_CALI_ALIN,
       cc.DE_CALI_ALIN      AS DE_CALI_ALIN,
       cl.NU_CANT_PRES      AS NU_CANT_PRES,
       sc.CO_SEGM_NEG       AS CO_SEGM_NEGO,
       sc.DE_SEGM_NEG       AS DE_SEGM_NEGO,
       sc.CO_SEGMENTACION   AS CO_SEGMENTACION,
       sg.DE_SEGMENTACION   AS DE_SEGMENTACION,
       cl.TI_PERSONA        AS TI_PERSONA,
       SYSDATE              AS FE_ACTU_MES
FROM SUBQ1 eg
      INNER JOIN ODS.MD_CLIENTES cl ON eg.CO_CLIENTE=cl.CO_CLIENTE
      LEFT JOIN ODS.HD_SEGMENTACION_CLIENTE PARTITION (P_HD_SEGMENT_CLIENTE_${FECHAPROCESOEDY_S_P}) sc ON eg.CO_CLIENTE = sc.CO_CLIENTE 
      LEFT JOIN ODS.HM_CALIFICACION_CLIENTE PARTITION (P_HM_CALIF_CLIENTE_${FECHAPROCESOEDY_S_P_M}) cc ON eg.CO_CLIENTE = cc.CO_CLIENTE
      LEFT JOIN ODS.MD_CLASIFICACION_DEPENDIENTE cd ON cd.CO_CLAS_DEPE = cl.CO_CLAS_DEPE
      LEFT JOIN ODS.MD_SEGMENTACION sg ON sg.CO_SEGMENTACION = sc.CO_SEGMENTACION
      LEFT JOIN ODS.MD_ESTADO_CIVIL ec ON ec.CO_ESTA_CIVI = cl.CO_ESTA_CIVI</sql>
