    <connection>OdsStgDS</connection>
    <sql>-- ==========
-- SOLICITUDES
-- ==========

SELECT TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) AS FE_PROCESO,
       a.NU_SOLICITUD AS NU_SOLICITUD,
       a.FE_REGI_SOLI AS FE_INGR_SOLI,
       a.CO_CLIENTE   AS CO_CLIENTE,
       b.TI_PERSONA   AS CO_TIPO_PERS,
       b.CO_CATE_PERS AS CO_CATE_PERS,
       b.CO_CLAS_DEPE AS CO_CLAS_DEPE,
       b.CO_ESTA_CIVI AS CO_ESTA_CIVI,
       TRUNC((TRUNC(c.FE_DESEMBOLSO) - TRUNC(b.FE_NACIMIENTO)) &#47; 365) AS NU_EDAD,
       CASE
         WHEN b.TI_PERSONA = &apos;J&apos; THEN &apos;P.JURIDICA&apos;
         WHEN (TRUNC(c.FE_DESEMBOLSO) - TRUNC(b.FE_NACIMIENTO)) &#47; 365 &lt;= 25 THEN &apos;&lt;0  -  25]&apos;
         WHEN (TRUNC(c.FE_DESEMBOLSO) - TRUNC(b.FE_NACIMIENTO)) &#47; 365 &lt;= 35 THEN &apos;&lt;25 -  35]&apos;
         WHEN (TRUNC(c.FE_DESEMBOLSO) - TRUNC(b.FE_NACIMIENTO)) &#47; 365 &lt;= 45 THEN &apos;&lt;35 -  45]&apos;
         WHEN (TRUNC(c.FE_DESEMBOLSO) - TRUNC(b.FE_NACIMIENTO)) &#47; 365 &gt; 45 THEN &apos;&lt;45 - M√ÅS&gt;&apos;
       END AS DE_RANG_EDAD,
       n.MO_SALARIO          AS MO_SALARIO,
       n.MO_VENT_NETA        AS MO_VENT_NETA,
       n.MO_UTIL_DISP        AS MO_UTIL_DISP,
       e.CO_SEGMENTACION     AS CO_SEGM_RIES,
       NVL(e.CO_SEGM_NEG, 0) AS CO_SEGM_NEGO,
       b.CO_ACTIVIDAD        AS CO_ACTI_INTE,
       a.CO_MONEDA           AS CO_MONEDA,
       a.CO_ESTA_SOLI        AS CO_ESTA_SOLI,
       CASE WHEN NVL(c.NU_LINE_CRED, 0) = 0 THEN 1 ELSE DECODE(a.FE_EVAL_SOLI, NULL, 0, 1) END AS IN_EVALUACION,
       CASE WHEN NVL(c.NU_LINE_CRED, 0) = 0 THEN 1 ELSE DECODE(a.FE_APRO_SOLI, NULL, 0, 1) END AS IN_APROBACION,
       DECODE(c.FE_DESEMBOLSO, NULL, 0, 1) AS IN_DESEMBOLSO,
       f.TI_VIVIENDA       AS TI_VIVI_DOMI,
       g.TI_VIVIENDA       AS TI_VIVI_CORR,
       h.TI_VIVIENDA       AS TI_VIVI_NEGO,
       i.TI_VIVIENDA       AS TI_VIVI_GARA,
       j.CO_ANALISTA       AS CO_FUNC_REGI_SOLI,
       a.CO_USUA_TOPA_REGI AS CO_USUA_TOPA_REGI,
       m.NO_ANALISTA       AS NO_FUNC_REGI_SOLI,
       m.CO_PUESTO         AS CO_PUES_REGI_SOLI,
       m.CO_CARGO          AS CO_CARG_REGI_SOLI,
       k.CO_ANALISTA       AS CO_FUNC_EVAL,
       a.CO_USUA_TOPA_EVAL AS CO_USUA_TOPA_EVAL,
       o.NO_ANALISTA       AS NO_FUNC_EVAL,
       o.CO_PUESTO         AS CO_PUES_EVAL,
       o.CO_CARGO          AS CO_CARG_EVAL,
       l.CO_ANALISTA       AS CO_FUNC_APRO,
       a.CO_USUA_TOPA_APRO AS CO_USUA_TOPA_APRO,
       p.NO_ANALISTA       AS NO_FUNC_APRO,
       p.CO_PUESTO         AS CO_PUES_APRO,
       p.CO_CARGO          AS CO_CARG_APRO,
       a.CO_SUCURSAL       AS CO_AGENCIA,
       d.DE_SUCURSAL       AS DE_AGENCIA,
       -- d.CO_TERRITORIO
       NULL                AS CO_TERRITORIO,
       -- d.CO_ZONA
       NULL                AS CO_ZONA,
       d.CO_REGION         AS CO_REGION,
       NULL                AS CO_CALI_RCC_MAXI,
       a.MO_PRESTAMO       AS MO_PRESTAMO,
       a.MO_CUOTA          AS MO_CUOTA,
       NVL(a.NU_PLAZ_APROB, a.NU_PLAZ_SOLI) AS NU_PLAZOS,
       a.NU_CUOTAS         AS NU_CUOTAS,
       a.VL_TASA           AS VL_TASA,
       SYSDATE             AS FE_ACTU_DIA
FROM   ODS.HD_SOLICITUDES PARTITION (P_SOLICITUDES_${FECHAPROCESOEDY_S_P}) a
       LEFT JOIN ODS.MD_CLIENTES b   ON a.CO_CLIENTE = b.CO_CLIENTE
       LEFT JOIN ODS.HD_DESEMBOLSO c ON a.NU_SOLICITUD = c.NU_SOLICITUD
       LEFT JOIN ODS.MD_SUCURSAL d   ON a.CO_SUCURSAL = d.CO_SUCURSAL
       LEFT JOIN ODS.HD_SEGMENTACION_CLIENTE PARTITION (P_HD_SEGMENT_CLIENTE_${FECHAPROCESOEDY_S_P}) e ON b.CO_CLIENTE = e.CO_CLIENTE
       LEFT JOIN ODS.MD_DIRECCIONES_CLIENTE f  ON b.CO_CLIENTE = f.CO_CLIENTE
                                              AND f.CO_FORMATO = &apos;P&apos; 
                                              AND f.CO_TIPO_DIRE = &apos;R&apos;
       LEFT JOIN ODS.MD_DIRECCIONES_CLIENTE g  ON b.CO_CLIENTE = g.CO_CLIENTE
                                              AND g.CO_TIPO_DIRE = &apos;C&apos;
       LEFT JOIN ODS.MD_DIRECCIONES_CLIENTE h  ON b.CO_CLIENTE = h.CO_CLIENTE
                                              AND h.CO_FORMATO = &apos;N&apos; 
                                              AND h.CO_TIPO_DIRE = &apos;R&apos;
                                              AND h.CO_NEGOCIO = 1
       LEFT JOIN ODS.MD_DIRECCIONES_CLIENTE i  ON b.CO_CLIENTE = i.CO_CLIENTE
                                              AND i.CO_TIPO_DIRE = &apos;G&apos;
       LEFT JOIN ODS.HD_PUESTO_CARGO_ANALISTA PARTITION (P_HD_PUES_CARG_ANAL_${FECHAPROCESOEDY_S_P}) j    ON a.CO_USUA_TOPA_REGI = j.CO_USUA_TOPA
       LEFT JOIN ODS.MD_ANALISTA m    ON j.CO_ANALISTA = m.CO_ANALISTA
       LEFT JOIN ODS.HD_PUESTO_CARGO_ANALISTA PARTITION (P_HD_PUES_CARG_ANAL_${FECHAPROCESOEDY_S_P}) k    ON a.CO_USUA_TOPA_EVAL = k.CO_USUA_TOPA
       LEFT JOIN ODS.MD_ANALISTA o    ON k.CO_ANALISTA = o.CO_ANALISTA
       LEFT JOIN ODS.HD_PUESTO_CARGO_ANALISTA PARTITION (P_HD_PUES_CARG_ANAL_${FECHAPROCESOEDY_S_P}) l    ON a.CO_USUA_TOPA_APRO = l.CO_USUA_TOPA
       LEFT JOIN ODS.MD_ANALISTA p    ON l.CO_ANALISTA = p.CO_ANALISTA
       LEFT JOIN ODS.MD_PERDIDAS_GANANCIAS n ON a.NU_SOLICITUD = n.NU_SOLICITUD
											AND n.NU_DOCUMENTO = b.NU_DOCUMENTO
											AND n.TI_DOCUMENTO = b.TI_DOCUMENTO
											AND n.NU_ORDINAL = 1</sql>
