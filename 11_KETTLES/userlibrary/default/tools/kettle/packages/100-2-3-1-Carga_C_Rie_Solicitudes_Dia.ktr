  <step>
    <name>Leo C_RIE_SOLICITUDES_DIA</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
