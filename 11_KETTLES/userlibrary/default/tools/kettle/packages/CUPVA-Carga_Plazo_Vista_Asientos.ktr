  <step>
    <name>Leo EDYFICAR.ASIENTOS WHERE EXISTS IN HISTORIA_VISTA, BS_HISTORIA_PLAZO , TMP_SALDOS_*</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>TopazRepDS</connection>
    <sql>WITH SUBQ1 AS(
              SELECT  hv.FECHA_PROCESADO AS FECHAPROCESOMOV, hv.SUCURSAL AS SUCURSALMOV, hv.ASIENTO AS NROASIENTOMOV  FROM EDYFICAR.HISTORIA_VISTA hv WHERE hv.EVENTO IN (&apos;A&apos;,&apos;M&apos;) GROUP BY hv.FECHA_PROCESADO, hv.SUCURSAL, hv.ASIENTO
              UNION 
              SELECT  bshp.FECHAPROCESOMOV, bshp.SUCURSALMOV, bshp.NROASIENTOMOV FROM EDYFICAR.BS_HISTORIA_PLAZO bshp WHERE bshp.TIPOMOV IN (&apos;A&apos;,&apos;P&apos;) AND bshp.TZ_LOCK=0 GROUP BY bshp.FECHAPROCESOMOV, bshp.SUCURSALMOV, bshp.NROASIENTOMOV
)           
SELECT
	   DISTINCT 
       a.FECHAPROCESO AS FECHAPROCESO,
       a.SUCURSAL AS SUCURSAL,
       a.ASIENTO AS ASIENTO,
       a.OPERACION AS OPERACION,
       a.HORAFIN AS HORAFIN,
       a.INIUSR AS INIUSR,
       a.HORAINICIO AS HORAINICIO,
       a.DESCRIPCION AS DESCRIPCION 
FROM EDYFICAR.ASIENTOS a 
                       JOIN SUBQ1 
                            ON a.FECHAPROCESO=SUBQ1.FECHAPROCESOMOV
                            AND a.SUCURSAL=SUBQ1.SUCURSALMOV
                            AND a.ASIENTO=SUBQ1.NROASIENTOMOV         
WHERE a.ESTADO=77
      AND a.TZ_LOCK=0
	  --AND a.FECHAPROCESO &lt;= TO_DATE(&apos;29&#47;06&#47;2017&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)
	  AND a.FECHAPROCESO &lt;= SYSDATE-1 </sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
     <cluster_schema/>
 <remotesteps>   <input>   </input>   <output>   </output> </remotesteps>    <GUI>
      <xloc>273</xloc>
      <yloc>233</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>Leo EDYFICAR.BS_HISTORIA_PLAZO WHERE EXISTS IN EDYFICAR.TMP_SALDOS_01</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>TopazRepDS</connection>
    <sql>WITH SUBQ1 AS (
          SELECT &#47;*+ INDEX_COMBINE(s1 IDX_07_TMP_SALDOS_01, IDX_17_TMP_SALDOS_01)*&#47;  s1.JTS_OID FROM EDYFICAR.TMP_SALDOS_01 s1 WHERE s1.TIP_PRO=5 AND s1.TZ_LOCK=0          
        ) 
SELECT  
    bshp.JTS_OID AS JTS_OID,
    bshp.FECHAVALOR AS FECHAVALOR,
    bshp.TIPOMOV AS TIPOMOV,
    bshp.NROASIENTOMOV AS NROASIENTOMOV,
    bshp.SUCURSALMOV AS SUCURSALMOV,
    bshp.FECHAPROCESOMOV AS FECHAPROCESOMOV,
    bshp.CAPITALPAGADO AS CAPITALPAGADO,
    bshp.INTERESPAGADO AS INTERESPAGADO,
    bshp.MORAPAGADA AS MORAPAGADA,
    bshp.saldos_jts_oid AS SALDOS_JTS_OID
FROM EDYFICAR.BS_HISTORIA_PLAZO bshp
                                    JOIN SUBQ1 s
                                         ON s.JTS_OID=bshp.SALDOS_JTS_OID     
WHERE bshp.TZ_LOCK=0
      AND bshp.TIPOMOV IN (&apos;A&apos;,&apos;P&apos;)
	  --AND bshp.FECHAPROCESOMOV &lt;= TO_DATE(&apos;29&#47;06&#47;2017&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)
	  AND bshp.FECHAPROCESOMOV &lt;= SYSDATE-1 </sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
