  <step>
    <name>Leo EDYFICAR.HISTORIA_VISTA  WHERE EXISTS EDYFICAR.TMP_SALDOS_04</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>TopazRepDS</connection>
    <sql>WITH SUBQ1 AS (
               SELECT s.JTS_OID FROM EDYFICAR.TMP_SALDOS_04 s WHERE s.C9314 IN (2,3) AND s.TZ_LOCK=0     
              )
SELECT hv.ASIENTO AS ASIENTO,
       hv.NUMERO_MOVIMIENTO AS NUMERO_MOVIMIENTO,
       hv.FECHA_PROCESADO AS FECHA_PROCESADO,
       hv.ORDINAL AS ORDINAL,
       hv.FECHA_VALOR AS FECHA_VALOR,
       hv.SUCURSAL AS SUCURSAL,
       hv.TIPO AS TIPO,
       hv.DEBITO_CREDITO AS DEBITO_CREDITO,
       hv.CONCEPTO AS CONCEPTO,
       hv.MONTO AS MONTO,
       hv.SALDO_JTS_OID AS SALDO_JTS_OID,
       hv.EVENTO AS EVENTO,
       hv.CAJA_DIARIO AS CAJA_DIARIO,
     hv.AJUSTE AS AJUSTE, 
     hv.CANAL AS CANAL,
     hv.TRVIRTUAL AS TRVIRTUAL,
     hv.EQUIVALENTEMN AS EQUIVALENTEMN,
     hv.TRTIPO AS TRTIPO
FROM EDYFICAR.HISTORIA_VISTA hv
                  JOIN SUBQ1
                       ON hv.saldo_jts_oid=SUBQ1.JTS_OID
WHERE 
    hv.EVENTO IN (&apos;A&apos;,&apos;M&apos;)
	--AND hv.FECHA_PROCESADO &lt;= TO_DATE(&apos;29&#47;06&#47;2017&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)
	AND hv.FECHA_PROCESADO &lt;= SYSDATE-1 </sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
