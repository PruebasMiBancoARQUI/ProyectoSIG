           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>SELECT IG.ID_MOVIMIENTO
      ,CASE  WHEN NVL(IG.CO_CLIENTE, 0) &gt; 0 THEN
                  IG.CO_CLIENTE
             ELSE S.C1803
       END                                          CO_CLIENTE
      ,CASE WHEN NVL(IG.CO_PRODUCTO, 0) &gt; 0 THEN
                 IG.CO_PRODUCTO
            ELSE S.PRODUCTO
       END                                          CO_PRODUCTO
      ,CASE WHEN NVL(IG.CO_ANALISTA,0) &gt; 0 THEN
                 IG.CO_ANALISTA
            ELSE S.C1670
       END                                          CO_ANALISTA
FROM   ODS.TP_BROAD_DETALLE  IG
       INNER JOIN STG.T_SALDOS_CONTA S
             ON S.CUENTA          = IG.NU_CUENTA
                AND S.SUCURSAL    = IG.CO_SUCURSAL
                AND S.C9314       IN (2, 3, 4)
WHERE  IG.NU_CUENTA &gt; 0
       AND IG.TI_CLAS_CUEN_CONT = &apos;GASTOS&apos;
       AND (IG.CO_PRODUCTO = 0 OR IG.CO_CLIENTE = 0 OR IG.CO_ANALISTA = 0)</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
