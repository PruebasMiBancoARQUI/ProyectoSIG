    <sql>SELECT 
	a.idtabla AS Id_Tabla,
	a.ordinal AS Ordinal,
	TO_NUMBER(SUBSTR(a.descripcion, 1, INSTR(a.descripcion, &apos;-&apos;) - 1)) AS COD_CLIENTE,
	SUBSTR(a.descripcion, INSTR(a.descripcion, &apos;-&apos;) + 1) AS NOMBRE_CLIENTE,
	b.idtabla   ID_TABLA_TIPO_ACREEDOR,
	b.ordinal    ORDINAL_TIPO_ACREEDOR,
	b.abreviatura AS COD_TIPO_ACREEDOR,
    b.descripcion AS DESC_TIPO_ACREEDOR
FROM EDYFICAR.TC_TABLADETABLAS a
	 LEFT OUTER JOIN EDYFICAR.TC_TABLADETABLAS b
		ON TO_NUMBER(b.idtabla) = TO_NUMBER(SUBSTR(a.abreviatura, 1, INSTR(a.abreviatura, &apos;,&apos;) - 1))
        AND TO_NUMBER(b.ordinal) = TO_NUMBER(SUBSTR(a.abreviatura, INSTR(a.abreviatura, &apos;,&apos;) + 1))
WHERE  
	a.idtabla = 338
    AND a.ordinal &gt; 0
    AND a.tz_lock = 0
	AND b.TZ_LOCK=0
ORDER BY COD_TIPO_ACREEDOR,a.ordinal
