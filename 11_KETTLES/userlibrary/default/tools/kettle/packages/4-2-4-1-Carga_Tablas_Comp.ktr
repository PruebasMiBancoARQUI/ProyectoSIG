  <step>
    <name>Leo Dias de Atraso de Plan Pagos</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>TopazRepDS</connection>
    <sql>SELECT
  sq.SALDOS_JTS_OID,
  SUM(sq.TOTA_DIAS_ATRASO) AS TOTA_DIAS_ATRASO,
  MAX(sq.MAX_DIAS_ATRASO) AS MAX_DIAS_ATRASO,
  SUM(sq.NRO_CUOTAS_VENC) AS NRO_CUOTAS_VENC,
  MAX(sq.DIAS_MORA_MES) AS DIAS_MORA_MES
FROM
(
  SELECT
    pp.SALDO_JTS_OID AS SALDOS_JTS_OID,
    (CASE WHEN ((pp.C2309+pp.C2310 + NVL((SELECT SUM(gpc.SALDO_GASTO) FROM EDYFICAR.GASTOS_POR_CUOTA gpc WHERE gpc.SALDOS_JTS_OID = pp.SALDO_JTS_OID AND gpc.NUMERO_CUOTA = pp.C2300 AND gpc.TZ_LOCK = 0),0))&lt;&gt;0 AND pp.C2302&lt;TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) THEN TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)-pp.C2302
         WHEN ((pp.C2309+pp.C2310+ NVL((SELECT SUM(gpc.SALDO_GASTO) FROM EDYFICAR.GASTOS_POR_CUOTA gpc WHERE gpc.SALDOS_JTS_OID = pp.SALDO_JTS_OID AND gpc.NUMERO_CUOTA = pp.C2300 AND gpc.TZ_LOCK = 0),0))=0 AND pp.FECHACANCELACION&gt;pp.C2302) THEN pp.FECHACANCELACION-pp.C2302
         ELSE 0
      END) AS TOTA_DIAS_ATRASO,
    (CASE WHEN ((pp.C2309+pp.C2310+ NVL((SELECT SUM(gpc.SALDO_GASTO) FROM EDYFICAR.GASTOS_POR_CUOTA gpc WHERE gpc.SALDOS_JTS_OID = pp.SALDO_JTS_OID AND gpc.NUMERO_CUOTA = pp.C2300 AND gpc.TZ_LOCK = 0),0))&lt;&gt;0 AND pp.C2302&lt;TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) THEN TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)-pp.C2302
         WHEN ((pp.C2309+pp.C2310+ NVL((SELECT SUM(gpc.SALDO_GASTO) FROM EDYFICAR.GASTOS_POR_CUOTA gpc WHERE gpc.SALDOS_JTS_OID = pp.SALDO_JTS_OID AND gpc.NUMERO_CUOTA = pp.C2300 AND gpc.TZ_LOCK = 0),0))=0 AND pp.FECHACANCELACION&gt;pp.C2302) THEN pp.FECHACANCELACION-pp.C2302
         ELSE 0
      END) AS MAX_DIAS_ATRASO,
    (CASE WHEN pp.C2302 &lt;= TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) AND (pp.C2309+pp.C2310+ NVL((SELECT SUM(gpc.SALDO_GASTO) FROM EDYFICAR.GASTOS_POR_CUOTA gpc WHERE gpc.SALDOS_JTS_OID = pp.SALDO_JTS_OID AND gpc.NUMERO_CUOTA = pp.C2300 AND gpc.TZ_LOCK = 0),0))&lt;&gt;0 THEN 1
           ELSE 0
      END) AS NRO_CUOTAS_VENC,
    (CASE
  		WHEN (((pp.C2309+pp.C2310+ NVL((SELECT SUM(gpc.SALDO_GASTO) FROM EDYFICAR.GASTOS_POR_CUOTA gpc WHERE gpc.SALDOS_JTS_OID = pp.SALDO_JTS_OID AND gpc.NUMERO_CUOTA = pp.C2300 AND gpc.TZ_LOCK = 0),0))&lt;&gt;0) AND pp.C2302 &lt; (to_date(&apos;01&#47;&apos;||to_char(TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;),&apos;mm&#47;yyyy&apos;),&apos;dd&#47;mm&#47;yyyy&apos;)-1)) THEN ((to_date(&apos;01&#47;&apos;||to_char(TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;),&apos;mm&#47;yyyy&apos;),&apos;dd&#47;mm&#47;yyyy&apos;)-1) - pp.C2302)
    	  	WHEN (((pp.C2309+pp.C2310+ NVL((SELECT SUM(gpc.SALDO_GASTO) FROM EDYFICAR.GASTOS_POR_CUOTA gpc WHERE gpc.SALDOS_JTS_OID = pp.SALDO_JTS_OID AND gpc.NUMERO_CUOTA = pp.C2300 AND gpc.TZ_LOCK = 0),0))=0) AND pp.FECHACANCELACION &gt; (to_date(&apos;01&#47;&apos;||to_char(TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;),&apos;mm&#47;yyyy&apos;),&apos;dd&#47;mm&#47;yyyy&apos;)-1) AND pp.C2302 &lt; (to_date(&apos;01&#47;&apos;||to_char(TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;),&apos;mm&#47;yyyy&apos;),&apos;dd&#47;mm&#47;yyyy&apos;)-1)) THEN ((to_date(&apos;01&#47;&apos;||to_char(TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;dd&#47;mm&#47;yyyy&apos;),&apos;mm&#47;yyyy&apos;),&apos;dd&#47;mm&#47;yyyy&apos;)-1) - pp.C2302)
  		ELSE 0
    	  END) AS DIAS_MORA_MES
  FROM EDYFICAR.BS_PLANPAGOS pp
  	 JOIN EDYFICAR.SALDOS s
  		ON pp.SALDO_JTS_OID = s.JTS_OID
  WHERE
  	s.C1604 &lt; 0 
  	AND pp.TZ_LOCK=0
) sq
GROUP BY sq.SALDOS_JTS_OID</sql>
