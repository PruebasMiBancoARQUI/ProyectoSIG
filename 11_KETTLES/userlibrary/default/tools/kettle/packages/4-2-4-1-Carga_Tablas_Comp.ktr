    <connection>TopazRepDS</connection>
    <sql>SELECT
	SAL.C1803 AS CO_CLIENTE,
	SAL.OPERACION AS CO_PRESTAMO,
	SAL.JTS_OID AS SALDOS_JTS_OID,
	SAL.C1604 AS MO_ACT,
	nvl((SELECT SUM(NVL(HIV.MONTO,0)) FROM EDYFICAR.HISTORIA_VISTA HIV WHERE HIV.SALDO_JTS_OID=SAL.JTS_OID AND HIV.DEBITO_CREDITO=&apos;C&apos;),0)	AS CREDITOS,
	nvl((SELECT SUM(NVL(HIV.MONTO,0)) FROM EDYFICAR.HISTORIA_VISTA HIV WHERE HIV.SALDO_JTS_OID=SAL.JTS_OID AND HIV.DEBITO_CREDITO=&apos;D&apos;),0)	AS DEBITOS
FROM EDYFICAR.SALDOS SAL
	 INNER JOIN EDYFICAR.CO_CONCEPCONT CON 
	 	ON CON.TZ_LOCK=0 
	 	AND SAL.CUENTA=CON.C7502 
	 	AND CON.C7500 IN (23000001,23000002) 
	 	AND SAL.TZ_LOCK=0 AND SAL.OPERACION&lt;&gt;0
WHERE
	SAL.TZ_LOCK=0
	AND CON.TZ_LOCK=0</sql>
