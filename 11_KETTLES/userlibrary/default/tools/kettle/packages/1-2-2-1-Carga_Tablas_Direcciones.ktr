  <step>
    <name>Leo Dimensiones de UBIGEO</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>SELECT
	LPAD(TO_CHAR(b.C1212*10000+b.C1214*100+b.C1215),6,&apos;0&apos;) AS CO_UBIGEO,
	b.C1212 AS CO_DEPARTAMENTO,
	UPPER(d.C1481) AS DE_DEPARTAMENTO,
	b.C1214 AS CO_PROVINCIA,
	UPPER(m.C1207) AS DE_PROVINCIA,
	b.C1215 AS CO_DISTRITO,
	UPPER(b.C1216) AS DE_DISTRITO,
	SYSDATE AS FE_ACTU_DIA 
FROM STG.T_TC_BARRIOS b
	 JOIN STG.T_TC_MUNICIPIOS m
	 	ON b.C1212 =m.C1205 --Codigo Departamento
	 	AND b.C1214=m.C1206	--Codigo Provincia
	 JOIN STG.T_TC_DEPARTAMENTOS d
	 	ON d.C1480=b.C1212 --Codigo Departamento</sql>
