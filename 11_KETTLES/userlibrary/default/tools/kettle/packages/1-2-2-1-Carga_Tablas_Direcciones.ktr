    <connection>OdsStgDS</connection>
    <sql>SELECT
   d.C0902 AS CO_CLIENTE,
   d.FORMATO AS CO_FORMATO,
   UPPER(o1.DESCRIPCION) AS DE_FORMATO,
   d.TIPODIRECCION AS CO_TIPO_DIRE,
   UPPER(o2.DESCRIPCION) AS DE_TIPO_DIRE,
   UPPER(d.DES_DIR_COM) AS DE_DIRECCION,
   LPAD(d.UBIGEO,6,&apos;0&apos;) AS CO_UBIGEO,
   d.TEL1 AS NU_TELE_1,
   d.TEL2 AS NU_TELE_2,
   d.CELULAR AS NU_CELULAR,
   d.TIPOVIVIENDA AS TI_VIVIENDA,
   UPPER(o3.DESCRIPCION) AS DE_VIVIENDA,
   0 AS CO_NEGOCIO,
   &apos; &apos; AS DE_NEGOCIO,
   &apos;.&apos; AS TI_DOCUMENTO,
   &apos;.&apos; AS NU_DOCUMENTO,
   &apos;.&apos; AS CO_PAIS_DOCU,
   CASE WHEN NVL( LENGTH(d.DES_DIR_COM),0) &gt; 4 THEN 1 ELSE 0 END AS DIR_VALIDA,
   d.TIPOZONA AS CO_TIPO_NUCL_URBA,
   d.EMAIL AS DE_EMAIL,
   d.REFERENCIA AS DE_REFERENCIA,
   d.NOMZONA AS DE_NUCL_URBA,
   d.FEC_REG AS FE_REGISTRO,
   SYSDATE AS FE_ACTU_DIA 
FROM STG.T_TC_DIRECCIONES_CLIENTE d
	 JOIN STG.T_OPCIONES o1 
	 	ON d.FORMATO=o1.OPCIONINTERNA
	 	AND o1.NUMERODECAMPO=6161
	 JOIN STG.T_OPCIONES o2
	 	ON d.TIPODIRECCION=o2.OPCIONINTERNA
	 	AND o2.NUMERODECAMPO=6162
     LEFT JOIN STG.T_OPCIONES o3
		ON d.TIPOVIVIENDA=o3.OPCIONINTERNA
		AND o3.NUMERODECAMPO=6185
</sql>
