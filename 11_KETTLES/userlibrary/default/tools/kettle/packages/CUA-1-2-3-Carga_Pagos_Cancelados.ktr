    <connection>OdsStgDS</connection>
    <sql>WITH CHARGES_DETAIL AS (
SELECT 
  cd.CUOTA AS NU_CUOTA,
  s.CUENTA AS NU_CUENTA,
  s.OPERACION AS NU_OPERACION,
  s.C1803 AS CO_CLIENTE,
  s.SUCURSAL AS CO_SUCURSAL,
  s.MONEDA AS CO_MONEDA,
  s.PRODUCTO AS CO_PRODUCTO,
  hp.NROASIENTOMOV AS NU_ASIENTO,
  hp.SUCURSALMOV AS CO_SUCU_MOVI,
  hp.FECHAPROCESOMOV AS FE_ASIENTO,
  hp.FECHAVALOR AS FE_VALO_PAGO,
  s.C1670 AS CO_ANALISTA,
  pp.C2302 AS FE_VENC_CUOT,
 MAX( CASE WHEN (hp.FECHAVALOR &gt; pp.C2302) THEN (hp.FECHAVALOR - pp.C2302)
     ELSE 0
  END) AS NU_DIAS_ATRA,
   sum( CASE WHEN c.TIPO_CARGO=1 THEN cd.TOTAL_PAGADO
       ELSE 0
    END) AS MO_COMI_PGDA,
  sum(CASE WHEN c.TIPO_CARGO=2 THEN cd.TOTAL_PAGADO
       ELSE 0
    END) AS MO_IMPU_PGDO,
  sum(CASE WHEN c.TIPO_CARGO=3 THEN cd.TOTAL_PAGADO
       ELSE 0
    END) AS MO_GAST_PGDO,
  sum(CASE WHEN c.TIPO_CARGO=4 THEN cd.TOTAL_PAGADO
       ELSE 0
    END) AS MO_SEGU_RIES_PGDO,
    sum(CASE WHEN c.TIPO_CARGO=5 THEN cd.TOTAL_PAGADO
       ELSE 0
    END) AS MO_SEGU_DEVG_PGDO,
    sum(CASE WHEN c.TIPO_CARGO=6 THEN cd.TOTAL_PAGADO
       ELSE 0
    END) AS MO_OTRO_SEGU_PGDO,
  sum(CASE WHEN c.TIPO_CARGO=0 THEN cd.TOTAL_PAGADO
       ELSE 0
    END) AS MO_OTRO_CARG_PGDO,
  s.JTS_OID AS IN_SALD_JTS_OID,
  CASE WHEN EXISTS (SELECT 1 FROM STG.T_CR_RECUPERCARTERA cr WHERE cr.NROPRESTAMO = s.CUENTA AND cr.FECHAOPER = hp.FECHAPROCESOMOV AND cr.TIPOOPER=&apos;C&apos;) THEN 1
    ELSE 0
  END AS IN_CONDONACION,
  NVL(htc.TIPO_CAMBIO_OFICIAL,0) AS VL_TIPO_CAMB_OFIC
FROM STG.T_BS_CHARGE_DETAIL cd
   JOIN STG.T_SALDOS s
     ON s.JTS_OID=cd.SALDO_JTS_OID
   JOIN STG.T_BS_HISTORIA_PLAZO hp
     ON cd.HP_JTS_OID=hp.JTS_OID
   JOIN STG.T_BS_PLANPAGOS pp
    ON pp.C2300=cd.CUOTA
     AND pp.SALDO_JTS_OID=cd.SALDO_JTS_OID
   JOIN STG.T_CI_CARGOS c
     ON cd.CARGO_ID=c.ID_CARGO
   &#47;*JOIN STG.T_CO_MONEDAS_01 m
     ON s.MONEDA=m.C6399*&#47;
   LEFT JOIN STG.T_HISTORICOTIPOSCAMBIO htc
		ON htc.MONEDA=s.MONEDA
			AND htc.FECHA_COTIZACION=hp.FECHAPROCESOMOV
WHERE
  s.C1604=0 
GROUP BY 
  cd.CUOTA,
  s.CUENTA ,
  s.OPERACION,
  s.C1803 ,
  s.SUCURSAL,
  s.MONEDA ,
  s.PRODUCTO,
  hp.NROASIENTOMOV,
  hp.SUCURSALMOV ,
  hp.FECHAPROCESOMOV,
  hp.FECHAVALOR,
  s.C1670,
  pp.C2302,
  s.JTS_OID,
  CASE WHEN EXISTS (SELECT 1 FROM STG.T_CR_RECUPERCARTERA cr WHERE cr.NROPRESTAMO = s.CUENTA AND cr.FECHAOPER = hp.FECHAPROCESOMOV AND cr.TIPOOPER=&apos;C&apos;) THEN 1
    ELSE 0
  END,
  NVL(htc.TIPO_CAMBIO_OFICIAL,0)
)
,PAYS_DETAIL AS(
SELECT
  pd.CUOTA AS NU_CUOTA,
  s.CUENTA AS NU_CUENTA,
  s.OPERACION AS NU_OPERACION,
  s.C1803 AS CO_CLIENTE,
  s.SUCURSAL AS CO_SUCURSAL,
  s.MONEDA AS CO_MONEDA,
  s.PRODUCTO AS CO_PRODUCTO,
  pd.NROASIENTOMOV AS NU_ASIENTO,
  pd.SUCURSALMVTO AS CO_SUCU_MOVI,
  pd.FECHAPROCESOMOV AS FE_ASIENTO,
  pd.FECHAVALOR AS FE_VALO_PAGO,
  s.C1670 AS CO_ANALISTA,
  max(pp.C2302) AS FE_VENC_CUOT,
  MAX(CASE WHEN (pd.FECHAVALOR &gt; pp.C2302) THEN (pd.FECHAVALOR - pp.C2302)
     ELSE 0
  END) AS NU_DIAS_ATRA,  
  --SUM(pd.CAPITALPAGADO) AS MO_CAPI_PGDO,
  SUM(pd.CAPITALPAGADO+ pd.CAPITALADELANTADO) AS MO_CAPI_PGDO,
  SUM(pd.INTERESPAGADO) AS MO_INTE_PGDO,
  --SUM(pd.MORAPAGADA) AS MO_MORA_PGDA,
  SUM(pd.MORAPAGADA +pd.INTERES_COMP_MORA) AS MO_MORA_PGDA,
  s.JTS_OID AS IN_SALD_JTS_OID,
  NVL(SCC.CRE_MOTIVOCANC,&apos; &apos;) AS CO_TIPO_CANC,
  SUM(pd.INTERES_COMP_MORA) AS MO_INTE_COMP_MORA,
  NVL(a.INIUSR,&apos; &apos;) AS CO_USUA_TOPA,
  a.HORAFIN AS TX_HORA_TERM_TRAN,
  a.HORAINICIO AS TX_HORA_INIC_TRAN,
  SUM(pd.MO_ITF_PAGA) AS MO_ITF_PAGA,
  SUM(pd.MO_OTRO_PAGO) AS MO_OTRO_PAGO,
  NVL(a.OPERACION,0) AS CO_OPERATIVA,
  NVL(htc.TIPO_CAMBIO_OFICIAL,0) AS VL_TIPO_CAMB_OFIC,
  NVL(pd.TI_CANAL,&apos; &apos;) AS TI_CANAL,
  NVL(pd.ID_CANAL,&apos; &apos;) AS ID_CANAL,
  pd.DE_CANA_EXTE  
FROM STG.T_BS_PAYS_DETAIL pd
   JOIN STG.T_SALDOS s
     ON s.JTS_OID=pd.SALDOS_JTS_OID
   JOIN STG.T_BS_PLANPAGOS pp
    ON pp.C2300=pd.CUOTA
     AND pp.SALDO_JTS_OID=pd.SALDOS_JTS_OID
   LEFT JOIN STG.T_ASIENTOS_03 a
     ON a.ASIENTO = pd.NROASIENTOMOV
    AND a.SUCURSAL = pd.SUCURSALMVTO
    AND a.FECHAPROCESO = pd.FECHAPROCESOMOV
	LEFT JOIN STG.T_HISTORICOTIPOSCAMBIO htc
		ON htc.MONEDA=s.MONEDA
			AND htc.FECHA_COTIZACION=pd.FECHAPROCESOMOV	 
   LEFT JOIN STG.T_SL_SOLICITUDCREDCANCELA SCC 
	 ON SCC.CRE_NUMSOLICITUD = s.C1704 
	AND SCC.CRE_SUCURSAL = s.SUCURSAL 
	AND SCC.CRE_PRODUCTO = s.PRODUCTO 
	AND SCC.CRE_CUENTA = s.CUENTA
	AND SCC.CRE_MONEDA = s.MONEDA 
	AND SCC.CRE_OPERACION = s.OPERACION 
	AND SCC.CRE_ORDINAL = s.ORDINAL
WHERE
  s.C1604=0
GROUP BY
  pd.CUOTA,
  s.CUENTA,
  s.OPERACION,
  s.C1803,
  s.SUCURSAL,
  s.MONEDA,
  s.PRODUCTO,
  pd.NROASIENTOMOV,
  pd.SUCURSALMVTO,
  pd.FECHAPROCESOMOV,
  pd.FECHAVALOR,
  s.C1670,
  s.JTS_OID,
  SCC.CRE_MOTIVOCANC,
  --pd.INTERES_COMP_MORA,
  a.INIUSR,
  a.HORAINICIO,
  a.HORAFIN,
  --pd.MO_ITF_PAGA,
  --pd.MO_OTRO_PAGO,
  a.OPERACION,
  --m.C6440,
  NVL(htc.TIPO_CAMBIO_OFICIAL,0),
  pd.TI_CANAL,
  pd.ID_CANAL,
  pd.DE_CANA_EXTE)


SELECT 
  nvl(cd.NU_CUOTA,pd.NU_CUOTA) AS NU_CUOTA,
  nvl(cd.NU_CUENTA,pd.NU_CUENTA) AS NU_PRESTAMO,
  nvl(cd.NU_OPERACION,pd.NU_OPERACION) AS NU_OPERACION,
  nvl(cd.CO_CLIENTE, pd.CO_CLIENTE) AS CO_CLIENTE,
  nvl(cd.CO_SUCURSAL,pd.CO_SUCURSAL) AS CO_SUCURSAL,
  nvl(cd.CO_MONEDA,pd.CO_MONEDA) AS CO_MONEDA,
  nvl(cd.CO_PRODUCTO,pd.CO_PRODUCTO) AS CO_PRODUCTO,
  nvl(cd.NU_ASIENTO,pd.NU_ASIENTO) AS NU_ASIENTO,
  nvl(cd.CO_SUCU_MOVI,pd.CO_SUCU_MOVI) AS CO_SUCU_MOVI,
  nvl(cd.FE_ASIENTO,pd.FE_ASIENTO) AS FE_ASIENTO,
  nvl(cd.FE_VALO_PAGO,pd.FE_VALO_PAGO) AS FE_VALO_PAGO,
  nvl(cd.CO_ANALISTA,pd.CO_ANALISTA) AS CO_ANALISTA,
  nvl(cd.FE_VENC_CUOT,pd.FE_VENC_CUOT) AS FE_VENC_CUOT,
  nvl(cd.NU_DIAS_ATRA,pd.NU_DIAS_ATRA) AS NU_DIAS_ATRA,
  nvl(pd.MO_CAPI_PGDO,0) AS MO_CAPI_PGDO,
  nvl(pd.MO_INTE_PGDO,0) AS MO_INTE_PGDO,
  nvl(pd.MO_MORA_PGDA,0) AS MO_MORA_PGDA,
  nvl(cd.MO_COMI_PGDA,0) AS MO_COMI_PGDA,
  nvl(cd.MO_IMPU_PGDO,0) AS MO_IMPU_PGDO,
  nvl(cd.MO_GAST_PGDO,0) AS MO_GAST_PGDO,
  nvl(cd.MO_SEGU_RIES_PGDO,0) AS MO_SEGU_RIES_PGDO,
  nvl(cd.MO_SEGU_DEVG_PGDO,0) AS MO_SEGU_DEVG_PGDO,
  nvl(cd.MO_OTRO_SEGU_PGDO,0) AS MO_OTRO_SEGU_PGDO,
  NVL(cd.MO_OTRO_CARG_PGDO,0) AS MO_OTRO_CARG_PGDO,
  nvl(cd.IN_SALD_JTS_OID,pd.IN_SALD_JTS_OID) AS IN_SALD_JTS_OID,
  nvl(cd.IN_CONDONACION,&apos;0&apos;) AS IN_CONDONACION,
  nvl(pd.CO_TIPO_CANC,&apos; &apos;) AS CO_TIPO_CANC,
  nvl(pd.MO_INTE_COMP_MORA,0) AS MO_INTE_COMP_MORA,
  nvl(pd.CO_USUA_TOPA,&apos; &apos;) AS CO_USUA_TOPA,
  pd.TX_HORA_TERM_TRAN AS TX_HORA_TERM_TRAN,
  pd.TX_HORA_INIC_TRAN AS TX_HORA_INIC_TRAN,
  NVL(pd.MO_ITF_PAGA,0) AS MO_ITF_PAGA,
  NVL(pd.MO_OTRO_PAGO,0) AS MO_OTRO_PAGO,
  nvl(pd.CO_OPERATIVA,0) AS CO_OPERATIVA,
  nvl(cd.VL_TIPO_CAMB_OFIC,pd.VL_TIPO_CAMB_OFIC) AS VL_TIPO_CAMB_OFIC,
  nvl(pd.TI_CANAL,&apos; &apos;) AS TI_CANAL,
  nvl(pd.ID_CANAL,&apos; &apos;) AS ID_CANAL,
  nvl(pd.DE_CANA_EXTE,&apos; &apos;)AS DE_CANA_EXTE,
  NVL(EXTRACT(YEAR FROM pd.FE_VALO_PAGO)*100 + EXTRACT(MONTH FROM pd.FE_VALO_PAGO),0) AS NU_PERI_VALO_MES,
  NVL(EXTRACT(YEAR FROM pd.FE_ASIENTO)*100 + EXTRACT(MONTH FROM pd.FE_ASIENTO),0) AS NU_PERI_PROC_MES,
  SYSDATE AS FE_ACTU_DIA
FROM CHARGES_DETAIL cd 
   FULL JOIN PAYS_DETAIL pd
     ON cd.NU_CUOTA = pd.NU_CUOTA
     AND cd.NU_CUENTA = pd.NU_CUENTA
     AND cd.NU_OPERACION = pd.NU_OPERACION
     AND cd.CO_CLIENTE = pd.CO_CLIENTE
     AND cd.CO_SUCURSAL = pd.CO_SUCURSAL
     AND cd.CO_MONEDA = pd.CO_MONEDA
     AND cd.CO_PRODUCTO = pd.CO_PRODUCTO
    AND cd.NU_ASIENTO = pd.NU_ASIENTO
    AND cd.CO_SUCU_MOVI = pd.CO_SUCU_MOVI
    AND cd.FE_ASIENTO = pd. FE_ASIENTO</sql>
    <limit>0</limit>
