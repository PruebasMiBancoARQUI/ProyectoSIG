  <step>
    <name>Leo HD_DESEMBOLSO</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>SELECT NVL(d.NU_PRES_ORIG,b.NU_PRES_CANC) AS NU_PRES_ORIG,
       NVL(d.NU_PERI_MES_ORIG,TO_NUMBER(TO_CHAR(c.FE_DESEMBOLSO,&apos;YYYYMM&apos;))) AS NU_PERI_MES_ORIG,
       NVL(d.CO_PROD_ORIG,b.CO_PROD_CANC) AS CO_PROD_ORIG,
       a.NU_PRESTAMO AS NU_PRES_FINA,
       TO_NUMBER(TO_CHAR(a.FE_DESEMBOLSO,&apos;YYYYMM&apos;)) AS NU_PERI_MES_FINA,
       b.VL_PORC_PRES_CANC*NVL(d.VL_PORC_PART,1) AS VL_PORC_PART, 
       SYSDATE AS FE_ACTU_DIA 
FROM  ODS.HD_DESEMBOLSO a 
      INNER JOIN ODS.HD_PRESTAMOS_REFINANCIADOS b ON b.NU_PRESTAMO  = a.NU_PRESTAMO 
      INNER JOIN ODS.HD_DESEMBOLSO c              ON c.NU_PRESTAMO  = b.NU_PRES_CANC 
      LEFT JOIN BDS.F_RIE_REF_EXTREMO d           ON d.NU_PRES_FINA = b.NU_PRES_CANC 
WHERE a.FE_DESEMBOLSO = TO_DATE(&apos;${FECHAPROCESOEDY_S_P}&apos;,&apos;YYYYMMDD&apos;)
      AND a.CO_REFINANCIACION IN (&apos;R&apos;,&apos;P&apos;)
      AND (
           (d.NU_PRES_ORIG IS NULL AND c.CO_REFINANCIACION NOT IN (&apos;R&apos;,&apos;P&apos;))
           OR
           (d.NU_PRES_ORIG IS NOT NULL)
          )
</sql>
