  <step>
    <name>Leo T_BS_PLANPAGOS y T_GASTOS_POR_CUOTA</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>SELECT
  pp.C2300 AS NU_CUOTA,
  s.CUENTA AS NU_PRESTAMO,
  s.OPERACION AS NU_OPERACION,
  s.C1803 AS CO_CLIENTE,
  s.SUCURSAL AS CO_SUCURSAL,
  s.MONEDA AS CO_MONEDA,
  s.PRODUCTO AS CO_PRODUCTO,
  s.C1670 AS CO_ANALISTA,
  pp.C2301 AS FE_INICIO,
  pp.C2302 AS FE_VENCIMIENTO,
  pp.C2304 AS MO_CAPITAL,
  pp.C2305 AS MO_INTERES,
  pp.C2309 AS MO_SALD_CAPI,
  pp.C2310 AS MO_SALD_INTE,
  nvl((SELECT SUM (gpc.IMPORTE_GASTO) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=1
   ),0) AS MO_COMI_IMPO,
  nvl((SELECT SUM(nvl(gpc.IMPORTE_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=2
   ),0) AS MO_IMPU_IMPO,
  nvl((SELECT SUM(nvl(gpc.IMPORTE_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=3
   ),0) AS MO_GAST_IMPO,
  nvl((SELECT SUM(nvl(gpc.IMPORTE_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=4
   ),0)AS MO_SEGU_RIES_IMPO,
  nvl((SELECT SUM(nvl(gpc.IMPORTE_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=5
   ),0) AS MO_SEGU_DEVG_IMPO,
   nvl((SELECT SUM(nvl(gpc.IMPORTE_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=6
   ),0) AS MO_OTRO_SEGU_IMPO,
   nvl((SELECT SUM(nvl(gpc.IMPORTE_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=0
   ),0) AS MO_OTRO_CARG_IMPO,
  nvl((SELECT SUM(nvl(gpc.SALDO_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=1
   ),0) AS MO_COMI_SALD,
  nvl((SELECT SUM(nvl(gpc.SALDO_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=2
   ),0) AS MO_IMPU_SALD,
  nvl((SELECT SUM(nvl(gpc.SALDO_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=3
   ),0) AS MO_GAST_SALD,
  nvl((SELECT SUM(nvl(gpc.SALDO_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=4
   ),0) AS MO_SEGU_RIES_SALD,
   nvl((SELECT SUM(nvl(gpc.SALDO_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=5
   ),0) AS MO_SEGU_DEVG_SALD,
   nvl((SELECT SUM(nvl(gpc.SALDO_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=6
   ),0) AS MO_OTRO_SEGU_SALD,
  nvl((SELECT SUM(nvl(gpc.SALDO_GASTO,0)) 
   FROM STG.T_GASTOS_POR_CUOTA gpc
        LEFT JOIN STG.T_CI_CARGOS c
             ON gpc.CARGO_ID=c.ID_CARGO
   WHERE
     pp.SALDO_JTS_OID=gpc.SALDOS_JTS_OID
     AND pp.C2300=gpc.NUMERO_CUOTA
     AND NVL(c.TIPO_CARGO,0)=0
   ),0) AS MO_OTRO_CARG_SALD,
  pp.SALDO_JTS_OID AS IN_SALD_JTS_OID,
  SYSDATE AS FE_ACTU_DIA
FROM STG.T_BS_PLANPAGOS pp
   JOIN STG.T_SALDOS s
     ON pp.SALDO_JTS_OID=s.JTS_OID
WHERE
  pp.C2309+pp.C2310&gt;0</sql>
