    <connection>OdsStgDS</connection>
    <sql>--@001 DMR CASO-22417 [Proyecto SIG] Incidencias Fact de Admisión: Corrección de duplicidad de Registros

WITH
     SUBQ1 AS
     (
      SELECT NU_SOLICITUD, NU_PRESTAMO, SUM(MO_CRED_INDI_CANC) AS MO_CANCELADO
      FROM ODS.HD_PRESTAMOS_REFINANCIADOS
      GROUP BY NU_SOLICITUD, NU_PRESTAMO
     ),
     SUBQ2 AS
     (
      SELECT NU_SOLICITUD, COUNT(1) AS NU_CANT_GARA 
      FROM ODS.MD_PERSONAS_SOLICITUD
      WHERE CO_ROL_PERS = &apos;G&apos;
      GROUP BY NU_SOLICITUD
     ),
     SUBQ3 AS
     (
      SELECT NU_SOLICITUD, COUNT(1) AS NU_CANT_CODE 
      FROM ODS.MD_PERSONAS_SOLICITUD
      WHERE CO_ROL_PERS = &apos;O&apos;
      GROUP BY NU_SOLICITUD
     )
SELECT a.NU_SOLICITUD, 
       b.NU_PRESTAMO,
       NVL(TO_NUMBER(TO_CHAR(a.FE_REGI_SOLI,&apos;YYYYMM&apos;)),0) AS NU_PERI_MES,
       a.FE_REGI_SOLI AS FE_INGR_SOLI, 
       NVL(a.FE_EVAL_SOLI, k.FE_EVAL_SOLI) AS FE_EVAL_SOLI, 
       NVL(a.FE_APRO_SOLI, k.FE_APRO_SOLI) AS FE_APRO_SOLI, 
       b.FE_DESEMBOLSO AS FE_DESEMBOLSO, 
       b.FE_APER_CUEN AS FE_APER_CUEN, 
       DECODE(NVL(a.FE_EVAL_SOLI, k.FE_EVAL_SOLI),NULL,0,1) AS IN_EVALUACION,
       DECODE(NVL(a.FE_APRO_SOLI, k.FE_APRO_SOLI),NULL,0,1) AS IN_APROBACION,
       DECODE(b.FE_DESEMBOLSO,NULL,0,1) AS IN_DESEMBOLSO,
       DECODE(TRIM(a.CO_TIPO_PRES),&apos;REFINANCIADO&apos;,1,0) AS IN_REFINANCIADO,
       CASE WHEN f.NU_CANT_GARA &gt; 0 THEN 1 ELSE 0 END AS IN_GARANTIA,
       CASE WHEN b.CO_REFINANCIACION NOT IN (&apos;R&apos;,&apos;P&apos;) AND UPPER(h.DE_PRODUCTO) LIKE &apos;%PRE%PAG%&apos; THEN 1 ELSE 0 END AS IN_PRES_PREP, 
       CASE WHEN g.NU_CANT_CODE &gt; 0 THEN 1 ELSE 0 END AS IN_CODEUDORES,
       a.CO_CLIENTE, 
       a.CO_ESTA_SOLI,
       a.CO_MONEDA,
       a.CO_TIPO_SOLI,
       a.CO_TIPO_PRES,
       a.CO_TIPO_GRAC,
       a.CO_TIPO_CRED,
       a.CO_PRODUCTO,
       0 AS IN_PRES_MIGR,
       b.NU_LINE_CRED,
       e.NU_SOLICITUD AS NU_SOLI_LINE_CRED,
       a.CO_FORM_DESE,
       a.CO_SUCURSAL  AS CO_AGENCIA,
       c.CO_ACTIVIDAD AS CO_ACTI_INTE,
       b.CO_GRUP_EXCE,
       a.NU_CUOT_GRAC AS NU_PERI_GRAC,
       a.VL_TASA AS VL_TASA_DESE,
       a.MO_SOLICITADO,
       a.MO_APROBADO,
       b.MO_DESEMBOLSADO,
       a.MO_DEUD_SOLI,
       b.MO_CUOTA,
       d.MO_CANCELADO AS MO_SALD_CRED_CANC, 
       a.NU_PLAZ_SOLI,
       a.NU_PLAZ_APROB AS NU_PLAZ_APRO, 
       b.NU_CUOTAS, 
       e.NU_DISP_LINE_CRED, 
       f.NU_CANT_GARA,
       g.NU_CANT_CODE,
       NULL AS NU_CANT_ENTI_ACRE,     --- Queda en stand by por el Proyecto Data Lake
       NULL AS NU_DIAS_MORA_RCC,      --- Queda en stand by por el Proyecto Data Lake
       NULL AS MO_DEUD_SIST_FINA,     --- Queda en stand by por el Proyecto Data Lake
       CASE
          WHEN a.FE_EVAL_SOLI IS NOT NULL THEN TRUNC(a.FE_EVAL_SOLI) - TRUNC(a.FE_REGI_SOLI)
          WHEN k.FE_EVAL_SOLI IS NOT NULL THEN TRUNC(k.FE_EVAL_SOLI) - TRUNC(k.FE_REGI_SOLI)
          ELSE NULL
       END AS NU_DIAS_PROC_EVAL,
       CASE
          WHEN a.FE_APRO_SOLI IS NOT NULL AND a.FE_EVAL_SOLI IS NOT NULL THEN TRUNC(a.FE_APRO_SOLI) - TRUNC(a.FE_EVAL_SOLI)
          WHEN k.FE_APRO_SOLI IS NOT NULL AND k.FE_EVAL_SOLI IS NOT NULL THEN TRUNC(k.FE_APRO_SOLI) - TRUNC(k.FE_EVAL_SOLI)
          ELSE NULL
       END AS NU_DIAS_PROC_APRO,
       CASE
          WHEN b.FE_DESEMBOLSO IS NOT NULL AND a.FE_APRO_SOLI IS NOT NULL THEN TRUNC(b.FE_DESEMBOLSO) - TRUNC(a.FE_APRO_SOLI)
          WHEN b.FE_DESEMBOLSO IS NOT NULL AND a.FE_REGI_SOLI IS NOT NULL THEN TRUNC(b.FE_DESEMBOLSO) - TRUNC(a.FE_REGI_SOLI)
          ELSE NULL
       END AS NU_DIAS_PROC_DESE,
       j.CO_MEDI_ORIG AS CO_CONV_SOLI,
       SYSDATE AS FE_ACTU_DIA
FROM ODS.HD_SOLICITUDES PARTITION (P_SOLICITUDES_${FECHAPROCESOEDY_S_P}) a
--@001 Ini
--   LEFT JOIN ODS.HD_DESEMBOLSO b ON b.NU_SOLICITUD = a.NU_SOLICITUD
     LEFT JOIN ODS.HD_DESEMBOLSO b ON b.NU_SOLICITUD = a.NU_SOLICITUD AND a.CO_ESTA_SOLI=88 AND b.FE_APER_CUEN=a.FE_PROCESO
--@001 Fin
     LEFT JOIN ODS.MD_CLIENTES c ON c.CO_CLIENTE = a.CO_CLIENTE
     LEFT JOIN SUBQ1 d ON d.NU_SOLICITUD = a.NU_SOLICITUD AND d.NU_PRESTAMO = b.NU_PRESTAMO
     LEFT JOIN ODS.MD_LINEA_CREDITO e ON e.NU_LINE_CRED = b.NU_LINE_CRED
     LEFT JOIN SUBQ2 f ON ((f.NU_SOLICITUD = a.NU_SOLICITUD AND b.NU_LINE_CRED = 0) OR (f.NU_SOLICITUD = e.NU_SOLICITUD AND b.NU_LINE_CRED &lt;&gt; 0))
     LEFT JOIN SUBQ3 g ON ((g.NU_SOLICITUD = a.NU_SOLICITUD AND b.NU_LINE_CRED = 0) OR (g.NU_SOLICITUD = e.NU_SOLICITUD AND b.NU_LINE_CRED &lt;&gt; 0))
     LEFT JOIN ODS.MD_PRODUCTO_01 h ON h.CO_PRODUCTO = a.CO_PRODUCTO
     LEFT JOIN ODS.MD_LINEA_CREDITO i ON i.NU_SOLICITUD = a.NU_SOLICITUD
     LEFT JOIN ODS.MD_APERTURA_PRESOLICITUD j ON j.NU_SOLICITUD = a.NU_SOLICITUD
     LEFT JOIN ODS.MD_SOLICITUDES k ON k.NU_SOLICITUD = e.NU_SOLICITUD
     LEFT JOIN BDS.T_RIE_ADMISION_CREDITOS m ON m.NU_SOLICITUD = a.NU_SOLICITUD
WHERE m.NU_SOLICITUD IS NULL
UNION ALL
SELECT a.NU_SOLICITUD,
       a.NU_PRESTAMO,
       a.NU_PERI_MES,
       a.FE_INGR_SOLI,
       a.FE_EVAL_SOLI,
       a.FE_APRO_SOLI,
       a.FE_DESEMBOLSO,
       a.FE_APER_CUEN,
       a.IN_EVALUACION,
       a.IN_APROBACION,
       a.IN_DESEMBOLSO,
       a.IN_REFINANCIADO,
       a.IN_GARANTIA,
       a.IN_PRES_PREP,
       a.IN_CODEUDORES,
       a.CO_CLIENTE,
       a.CO_ESTA_SOLI,
       a.CO_MONEDA,
       a.CO_TIPO_SOLI,
       a.CO_TIPO_PRES,
       a.CO_TIPO_GRAC,
       a.CO_TIPO_CRED,
       a.CO_PRODUCTO,
       a.IN_PRES_MIGR,
       a.NU_LINE_CRED,
       a.NU_SOLI_LINE_CRED,
       a.CO_FORM_DESE,
       a.CO_AGENCIA,
       a.CO_ACTI_INTE,
       a.CO_GRUP_EXCE,
       a.NU_PERI_GRAC,
       a.VL_TASA_DESE,
       a.MO_SOLICITADO,
       a.MO_APROBADO,
       a.MO_DESEMBOLSADO,
       a.MO_DEUD_SOLI,
       a.MO_CUOTA,
       a.MO_SALD_CRED_CANC,
       a.NU_PLAZ_SOLI,
       a.NU_PLAZ_APRO,
       a.NU_CUOTAS,
       a.NU_DISP_LINE_CRED,
       a.NU_CANT_GARA,
       a.NU_CANT_CODE,
       a.NU_CANT_ENTI_ACRE,
       a.NU_DIAS_MORA_RCC,
       a.MO_DEUD_SIST_FINA,
       a.NU_DIAS_PROC_EVAL,
       a.NU_DIAS_PROC_APRO,
       a.NU_DIAS_PROC_DESE,
       a.CO_CONV_SOLI,
       a.FE_ACTU_DIA
FROM BDS.T_RIE_ADMISION_CREDITOS a</sql>
