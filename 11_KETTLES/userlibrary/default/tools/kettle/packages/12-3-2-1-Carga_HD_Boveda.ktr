  <step>
    <name>Leo HD_BOVEDA</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OdsStgDS</connection>
    <sql>SELECT
       TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) AS FE_PROCESO,
       a.FECHAPROCESO AS FE_PROCESO_ASIENTO,
       a.SUCURSAL AS CO_SUCURSAL,
       EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100 + EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) AS NU_PERI_MES,  
       MIN(a.ASIENTO) AS NU_ASIE_APER,
       a.OPERACION AS CO_OPER_APER,
       MIN(a.HORAINICIO) AS TX_HORA_APER,
       b.ASIENTO AS NU_ASIE_CIER,
       b.OPERACION AS CO_OPER_CIER,
       MAX(b.HORAINICIO) AS TX_HORA_CIER,
       SYSDATE AS FE_ACTU_DIA
FROM STG.T_ASIENTOS_05 a
              FULL OUTER JOIN STG.T_ASIENTOS_05 b ON  b.SUCURSAL = a.SUCURSAL AND b.FECHAPROCESO = a.FECHAPROCESO  AND b.OPERACION = 4721
WHERE 
    a.OPERACION = 4724   
GROUP BY TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;) , a.FECHAPROCESO , a.SUCURSAL, EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100 + EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)), a.OPERACION, b.ASIENTO, b.OPERACION

UNION 

SELECT
       TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;) AS FE_PROCESO,
       a.FECHAPROCESO AS FE_PROCESO_ASIENTO,
       a.SUCURSAL AS CO_SUCURSAL,
       EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100 + EXTRACT(MONTH FROM TO_DATE(&apos;30&#47;06&#47;2017&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)) AS NU_PERI_MES,  
       MIN(b.ASIENTO) AS NU_ASIE_APER,
       b.OPERACION AS CO_OPER_APER,
       MIN(b.HORAINICIO) AS TX_HORA_APER,
       a.ASIENTO AS NU_ASIE_CIER,
       a.OPERACION AS CO_OPER_CIER,
       MAX(a.HORAINICIO) AS TX_HORA_CIER,
       SYSDATE AS FE_ACTU_DIA
FROM STG.T_ASIENTOS_05 a
            FULL OUTER  JOIN STG.T_ASIENTOS_05 b ON  b.SUCURSAL = a.SUCURSAL AND b.FECHAPROCESO = a.FECHAPROCESO  AND b.OPERACION = 4724
WHERE 
    a.OPERACION = 4721  
GROUP BY TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;) , a.FECHAPROCESO , a.SUCURSAL, EXTRACT(YEAR FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;))*100 + EXTRACT(MONTH FROM TO_DATE(&apos;${FECHAPROCESOEDY_S}&apos;,&apos;DD&#47;MM&#47;YYYY&apos;)), a.OPERACION, a.ASIENTO, b.OPERACION
</sql>
